	</xsd:element>	
	<!-- NOT USED FOR 7.0 -->
    <!-- ================================================================ -->
	<!-- <kp-related-resources> element                                         												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-related-resources">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of knowledge path related resources</title>
				<desc>Collection of related resources (each containing resource links) that educate users on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="kp-related-resource" minOccurs="1" maxOccurs="6"/> <!-- 6 agreed to by Gretchen -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
    <!-- ================================================================ -->
	<!-- <kp-resource> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-resource">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A knowledge path resource</title>
				<desc>Knowledge path resource title and link for a specific on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="kp-resource-title" type="text-with-special-chars"/>
				<xsd:element name="kp-resource-url" type="kp-resource-url-eld" />
				<xsd:element name="kp-resource-note" type="text-with-special-chars"/>
			</xsd:sequence>
			<xsd:attribute name="kp-resource-type" type="resource-type" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the action assoc. with the resource, such as "Read" for articles and tutorials, "Listen" for podcasts, etc.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="kp-resource-id" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the CMA ID of the resource.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="kp-resource-registration" type="resource-registration" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the CMA ID of the resource.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
   <!-- NOT USED FOR 7.0 -->
    <!-- ================================================================ -->
	<!-- <kp-resource-url-eld> complexType                                   												   -->
	<!-- ================================================================ -->	
       <xsd:complexType name="kp-resource-url-eld">
          <xsd:simpleContent>
	         <xsd:extension base="xsd:string">	            
		        <xsd:annotation>
			       <xsd:documentation xml:lang="en">
				      <title>kp-resource-url-eld</title>
				      <desc>Defines the url for the resource, also indicating if the url is for an external link doc (ELD) or not.</desc>
			       </xsd:documentation>
		        </xsd:annotation>
		        <xsd:attribute name="eld" type="yesno" use="required">
			          <xsd:annotation>
				         <xsd:documentation xml:lang="en">
					     <desc>Allows you specify whether the resource url points to an ELD or not.</desc>
				      </xsd:documentation>
			       </xsd:annotation>
			    </xsd:attribute>
			 </xsd:extension>		
		  </xsd:simpleContent>		
	   </xsd:complexType>	
    <!-- NOT USED FOR 7.0 -->
    <!-- ================================================================ -->
	<!-- <kp-resources> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-resources">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of knowledge path resources</title>
				<desc>Collection of resources (each containing resource links) that educate users on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="kp-resource" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
    <!-- NOT USED FOR 7.0 -->
    <!-- ================================================================ -->
	<!-- <kp-step> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-step">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A knowledge path step</title>
				<desc>A knowledge path step containing a link to a resource</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="heading" />
				<xsd:element ref="kp-step-abstract" />
				<xsd:element ref="kp-resources" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>		
	<!-- NOT USED FOR 7.0 -->
    <!-- ================================================================ -->
	<!-- <kp-steps> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-steps">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of knowledge path steps</title>
				<desc>Collection of steps (each containing resource links) that educate users on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="kp-step" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>	
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <kp-step-abstract element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="kp-step-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A abstract for a knowledge path step.</title>
				<desc>An abstract used to summarize a knowledge path step.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a" />
				<xsd:element ref="br" />
				<xsd:element ref="em" />
				<xsd:element ref="strong" />
				<xsd:element ref="sub" />
				<xsd:element ref="sup" />
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
	    </xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <leadspace> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace</title>
				<desc>Defines a leadspace image for a featured content module.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="leadspace-elements-group"/>
			<xsd:attribute name="multicolumn" type="yesno" default="no" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify a multicolumn leadspace image</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <leadspace-decorative> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace-decorative">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace decorative</title>
				<desc>Defines a leadspace image for the featured content module that is for decorative use only and has no anchor link.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="image-url" type="xsd:string"/>
				<xsd:element name="image-alt" type="xsd:string" minOccurs="0"/>
				<xsd:element name="height" type="xsd:integer" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">
							<desc>Optionally, defines the height, in pixels, of the leadspace image.</desc>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="multicolumn" type="yesno" default="no" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify a multicolumn leadspace image</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
