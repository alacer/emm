	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- skill-level - Indicates complexity and depth of content contained in the document, from general background information for novices to advanced technical details for more experienced audience members         -->
	<!-- ================================================================ -->
	<xsd:simpleType name="skill-level">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>Indicates complexity and depth of content contained in the document, from general background information for novices to advanced technical details for more experienced audience members.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Limited knowledge, no experience</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>General knowledge, limited experience</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Applied knowledge, can perform routine tasks without assistance</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>In-depth knowledge, can perform all tasks without assitance</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="5">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Extensive knowledge and experience</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- summary-type - Indicates complexity and depth of content contained in the document, from general background information for novices to advanced technical details for more experienced audience members -->
	<!-- ================================================================ -->
	<xsd:simpleType name="summary-type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>Describes internal summary types for new summary type.  These types are needed for conditional stylesheet coding.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="briefing">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A briefing summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="chat">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A chat summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="demo">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<!-- Post xM (for V17) 11 09 11 egd: Updated the demo summary description to distinguish from the new rich media summary type. -->
						<desc>A demo summary page.  Used for on-demand demos.  For other rich media, including videos and podcasts, use the rich media summary.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="download">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A download summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="longdoc">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A long document (usually an article or white paper) summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="podcast">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A podcast summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="presentation">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A presentation summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="registration">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A registration summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!-- Post xM (for V17) 11 09 11 egd: Added the rich media summary type -->
			<xsd:enumeration value="rich media">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A summary page for an audio (podcast), video, or both.  The demo summary page should be used for on-demand demos.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="sample">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A sample (usually sample application) summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="specification">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A specification and/or standard summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="workshop">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A workshop summary page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
