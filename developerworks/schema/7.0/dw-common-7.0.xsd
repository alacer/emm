				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="style" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS style to be applied to this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the width of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="valign">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
