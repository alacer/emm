	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- vertical - vertical alignment values                             -->
	<!-- ================================================================ -->
	<xsd:simpleType name="vertical">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>The vertical alignment of the content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="bottom">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Vertically aligns the content along the bottom.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="middle">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Vertically aligns the content in the middle.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="top">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Vertically aligns the content at the top.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- webFeedType - now two kinds, rss and atom                                      -->
	<!-- ================================================================ -->
	<xsd:simpleType name="webFeedType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="rss"/>
			<xsd:enumeration value="atom"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- xslfoColor - one of the 16 color names from XSL-FO.              -->
	<!-- ================================================================ -->
	<xsd:simpleType name="xslfoColor">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>One of the sixteen color names defined in the XSL-FO standard, or a hexadecimal color code, as in <code type="inline">#FF00FF</code> or <code type="inline">FF00FF</code>.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="aqua"/>
			<xsd:pattern value="black"/>
			<xsd:pattern value="blue"/>
			<xsd:pattern value="gray"/>
			<xsd:pattern value="fuchsia"/>
			<xsd:pattern value="gray"/>
			<xsd:pattern value="green"/>
			<xsd:pattern value="lime"/>
			<xsd:pattern value="maroon"/>
			<xsd:pattern value="navy"/>
			<xsd:pattern value="olive"/>
			<xsd:pattern value="purple"/>
			<xsd:pattern value="red"/>
			<xsd:pattern value="silver"/>
			<xsd:pattern value="teal"/>
			<xsd:pattern value="white"/>
			<xsd:pattern value="yellow"/>
			<xsd:pattern value="[#]?[A-Fa-f0-9]{6}"/>
		</xsd:restriction>		
	</xsd:simpleType>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- yesno - a simple yes or no.                                      -->
	<!-- ================================================================ -->
	<xsd:simpleType name="yesno">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="yes"/>
			<xsd:enumeration value="no"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- year - the value for year must be reasonable               -->
	<!-- ================================================================ -->
	<!-- 6.0 xM R1 12/20/10 jpp: Updated maxInclusive value for year to 2020 -->	
	<xsd:simpleType name="year">	
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:minInclusive value="1996"/>
			<xsd:maxInclusive value="2020"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- zeroOrOne - either 0 or 1.                                       -->
	<!-- ================================================================ -->
	<xsd:simpleType name="zeroOrOne">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>Whether this item has a border.  A value of <code type="inline">0</code> means no border, <code type="inline">1</code> means there will be one.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- ELEMENT GROUPS -->
	<!-- ================================================================ -->
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <common-doc-group>                                                -->
	<!-- ================================================================ -->	
	<xsd:group name="common-doc-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the content of a document</title>
				<desc>Defines the content of a document.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="a"/>
			<xsd:element ref="b"/>
			<xsd:element ref="blockquote"/>
			<xsd:element ref="br"/>
			<xsd:element ref="code"/>
			<xsd:element ref="dl"/>
			<xsd:element ref="em"/>
			<xsd:element ref="figure"/>
			<xsd:element ref="heading"/>
			<xsd:element ref="i"/>
