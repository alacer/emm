					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="feed-url" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The URL of the Web feed.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="feed-type" type="webFeedType" default="rss">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The type of Web feed, RSS or Atom.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
