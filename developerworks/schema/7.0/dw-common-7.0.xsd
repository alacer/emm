	<!-- Project Defiant jpp 10/31/11: Currently not used; leadspace-results element left here for possible FUTURE USE -->
	<!-- Project Defiant jpp 10/06/11: Added leadspace-results element -->
	<!-- USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <leadspace-results> element        		                      												-->
	<!-- ================================================================ -->	
	<xsd:element name="leadspace-results">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace results</title>
				<desc>Defines the leadspace area for a developerWorks results page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="background-image-url" type="xsd:string"/>
				<xsd:element name="height" type="xsd:integer" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">
							<desc>Optionally, defines the height, in pixels, of the leadspace image.</desc>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="page-title" type="xsd:string"/>
				<xsd:element name="page-subtitle" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>		
