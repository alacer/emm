	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <page> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="page">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines a page in a multi-page document.</title>
				<desc>Defines a page in a multi-page document like generic-landing-page-pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- start of elements used if there are no subpages -->
				<xsd:element ref="pageinfo" minOccurs="0"/>
				<xsd:element name="content" type="page-subpage-elements-type" minOccurs="0"/>
				<!-- end of elements used if there are no subpages -->
				<xsd:element ref="subpage" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="tab-position" type="tabposition" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify the relative position of the tab for this page in the primary navigation.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="content-space-secondary-navigation" type="content-space-navigation-secondary" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the type of secondary navigation used on the page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="output" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether an output file should be generated for this page link.  Default is 'yes'.  If an output file is not generated (output='no'), it is 
            assumed that the page is generated by an application or remote server.  While the page file may be generated by an application or remote server,
            it must be visually "persistent" with the other pages in the pagegroup.  The page file must look as if it were part of the pagegroup in all respects.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <page-hidef> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="page-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines a page in a high definition multi-page document.</title>
				<desc>Defines a page in a high definition multi-page document like generic-landing-pagegroup-hidef.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- start of elements used if there are no subpages -->
				<xsd:element ref="pageinfo" minOccurs="0"/>
				<xsd:element name="content" type="page-subpage-elements-hidef-type" minOccurs="0"/>
				<!-- end of elements used if there are no subpages -->
				<xsd:element ref="subpage-hidef" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="tab-position" type="tabposition" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify the relative position of the tab for this page in the primary navigation.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="content-space-secondary-navigation" type="content-space-navigation-secondary" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the type of secondary navigation used on the page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="output" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether content should be generated for this page link.  Default is 'yes'.  If content is not being generated (output='no'), it is 
            assumed that the page is generated by an application or remote server.  While the page file may be generated by an application or remote server,
            it must be visually "persistent" with the other pages in the pagegroup.  The referenced page must look as if it were part of the pagegroup in all respects.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <page-subtitle> element                                               -->
