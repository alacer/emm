			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="i"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <page-title-hidef> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="page-title-hidef">
		<xsd:annotation>
