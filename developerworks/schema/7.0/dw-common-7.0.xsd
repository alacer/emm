	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <trademark> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="trademark">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Trademark attribution</title>
				<desc>Trademark statements are used to give proper (legal) attribution to content displayed on dW with permission from the trademark owner.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
				<!-- <xsd:element ref="common-doc"/> -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <trademarks> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="trademarks">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Trademarks</title>
				<desc>Contains one or more trademark elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="heading" minOccurs="0"/>
				<xsd:element ref="trademark" maxOccurs="unbounded"/>
				<!-- Trademarks may appear under a heading or not -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <translator> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="translator">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Translated by</title>
				<desc>Define information about person/company who translated the content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0">
				<xsd:element name="translator-name" type="dw-person-name" minOccurs="0"/>
				<xsd:element ref="company-name" minOccurs="0"/>
			</xsd:choice>
			<xsd:attribute name="email" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The translator's email address.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="company-website" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The company's web site address.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <twisty> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty section</title>
				<desc>Defines a twisty section within a module</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="twisty-heading" type="link-name"/>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element name="twisty-abstract" minOccurs="0"/>
						<xsd:element ref="twisty-column" minOccurs="2" maxOccurs="3"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-html-body"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-level-two" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <twisty-abstract> element                                                                                -->
	<!-- ================================================================ -->
	<xsd:element name="twisty-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty abstract</title>
				<desc>Descriptive abstract for a twisty section that allows limited HTML tags for highlighting, simple formatting, and special characters.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="em"/>
				<xsd:element ref="p"/>
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <twisty-column> element        		         								        				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-column">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty column</title>
				<desc>Defines a column within a twisty section.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="link-list"/>
				<xsd:element ref="forward-link-list"/>				
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <twisty-html-body> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-html-body">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty HTML body</title>
				<desc>Defines an HTML body section for a twisty that supports free-form HTML tags</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="twisty-body-elements-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <twisty-level-two> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-level-two">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Second level twisty</title>
				<desc>Defines second level twisty beneath a primary twisty</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="twisty-heading" type="link-name"/>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element name="twisty-abstract" minOccurs="0"/>
						<xsd:element ref="twisty-column" minOccurs="2" maxOccurs="2"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-html-body"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-level-three" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <twisty-level-three> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-level-three">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Third level twisty</title>
				<desc>Defines third level twisty beneath a secondary twisty</desc>
			</xsd:documentation>
		</xsd:annotation>
