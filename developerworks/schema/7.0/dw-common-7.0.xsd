	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <p> (paragraph) element                                          -->
	<!-- ================================================================ -->
	<xsd:element name="p">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a paragraph</title>
				<desc>Defines a paragraph of text.  </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- Maverick 6.0 R2 jpp-egd 061509: Removing dl as a child of p.  Element is allowed within docbody. -->
				<!-- <xsd:element ref="dl"/> -->
				<xsd:element ref="em"/>
				<xsd:element ref="font"/>
				<xsd:element ref="i"/>
				<xsd:element ref="img"/>
				<!-- Maverick 6.0 R2 jpp-egd 061509: Removing ol and sidebar as children of p.  Elements are allowed within docbody. -->
				<!-- <xsd:element ref="ol"/>
				<xsd:element ref="sidebar"/>  -->
				<xsd:element ref="span"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<!-- Maverick 6.0 R2 jpp-egd 061509: Removing table and ul as children of p.  Elements are allowed within docbody. -->
				<!-- <xsd:element ref="table"/>
				<xsd:element ref="ul"/> -->
				<xsd:group ref="specialCharacters"/>
				<!-- <font> is evil, must be killed -->
			    <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the alignment for this paragraph.</desc>
