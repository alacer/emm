	<!-- <sidebar> element                                                -->
	<!-- ================================================================ -->
	<xsd:element name="sidebar">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a sidebar for this article</title>
				<desc>Sideline thought or concept that is not part of the general flow of the document.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="include"/>
				</xsd:sequence>
				<xsd:choice minOccurs="0" maxOccurs="unbounded">
					<xsd:element ref="a"/>
					<xsd:element ref="b"/>
					<xsd:element ref="br"/>
					<xsd:element ref="code"/>
					<xsd:element ref="dl"/>
					<xsd:element ref="em"/>
					<xsd:element ref="heading"/>
					<xsd:element ref="i"/>
					<xsd:element ref="ol"/>
					<xsd:element ref="p"/>
					<xsd:element ref="strong"/>
					<xsd:element ref="sub"/>
					<xsd:element ref="sup"/>
					<xsd:element ref="ul"/>
					<xsd:group ref="specialCharacters"/>
			        <xsd:element ref="xref"/>
				</xsd:choice>
			</xsd:choice>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The width in pixels that should be used for the sidebar.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The horizontal alignment of the sidebar. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <sidebar-custom> element                                                -->
	<!-- ================================================================ -->
	<xsd:element name="sidebar-custom">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a custom sidebar for this content</title>
				<desc>Restricted sidebar for special uses; format is pre-defined.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="related-resource" type="link" minOccurs="1" maxOccurs="3"/>
			</xsd:sequence>
			<xsd:attribute name="type" type="sidebar-custom-types" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies the type of custom sidebar to generate.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <span> element   -->
	<!-- ================================================================ -->
	<xsd:element name="span">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a span tag</title>
				<desc>Defines a span tag.  Typical use is to display the text 'New' in the ibm.com highlight font in order to highlight new content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="b"/>
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the CSS class to be used for this span tag.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <special-offers> type                                                 -->
	<!-- ================================================================ -->
	<xsd:element name="special-offers">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>special-offers-section</title>
				<desc>Defines the element for the special offers section of the page</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="content-area-name" type="content-area" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The content area associated with the special offer.  To call the global special offers include or a custom special offers include, use the default value, none.  To call the special offers for a content area, select the content area.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="custom-file-name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Provides a relative URL path for a custom special offers include: /developerworks/inc/[filename]</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <strong> (strong) element                                                                                     -->
	<!-- ================================================================ -->
	<xsd:element name="strong">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines important text</title>
				<desc>Defines text that will appear in an emphasized font.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">			
				<xsd:element ref="a"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="em"/>
				<xsd:element ref="span"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			    <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <sub> (subscript) element                                        -->
	<!-- ================================================================ -->
	<xsd:element name="sub" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define subscript text</title>
				<desc>Defines text that should appear in subscript font.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <subpage> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="subpage">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines a subpage in a multi-page document.</title>
				<desc>Defines a subpage in a multi-page document like generic-landing-page-pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="subpageinfo" minOccurs="0"/>
				<xsd:element name="content" type="page-subpage-elements-type" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="persistent" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether the subpage is a persistent link (a subpage within the pagegroup) or is a non-persistent link to an outside page.
            Non-persistent links are allowed inside of a content link list.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <subpage-hidef> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="subpage-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines a subpage in a high definition multi-page document.</title>
				<desc>Defines a subpage in a high definition multi-page document like generic-landing-pagegroup-hidef.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="subpageinfo" minOccurs="0"/>
				<xsd:element name="content" type="page-subpage-elements-hidef-type" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="persistent" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether the subpage is a persistent link (a subpage within the pagegroup) or is a non-persistent link to an outside page.
            Non-persistent links are allowed inside of a content link list.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <subpageinfo> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="subpageinfo">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines the subpageinfo element.</title>
				<desc>Defines the name of the primary navigation item and the name and URL of the secondary navigation item for this page</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="primary-nav-name" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the name of the primary navigation item for this page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="secondary-nav-name" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the name of the secondary navigation item for this page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="secondary-nav-url" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the URL of the primary navigation item for this page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="page-end-btt-link" type="yesno" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify whether a back-to-top link should be automatically added to the bottom of the subpage.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <subtitle> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="subtitle">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a subtitle for the article</title>
				<desc>Secondary title that may provide more information about the document, and/or contain catchy, creative wording in order to stimulate interest in the document.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="i"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <sup> (superscript) element                                      -->
	<!-- ================================================================ -->
	<xsd:element name="sup" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define superscript text</title>
				<desc>Defines text that should appear in superscript font.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed system-requirements (summary) element as it is no longer used in 6.0 -->
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <table> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="table">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a table</title>
				<desc>Defines a table.  In addition to formatting tabular information, you can use tables to have more control over the layout of information.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="caption" minOccurs="0"/>
				<xsd:element ref="thead" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="tr" maxOccurs="unbounded"/>
				<xsd:element ref="tfoot" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="border" type="zeroOrOne">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Whether or not this table has a border.  </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="cellpadding" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The space in pixels that is added to all sides of the cells in the table.  A value of <code type="inline">cellpadding="3"</code> means none of the cells' contents will be closer than 3 pixels to the edge of the cells.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="cellspacing" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The space in pixels that is added between the cells in the table. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS class to be applied to this table</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="cols">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The dimensions of the columns in the table.  For example, the markukp <code type="inline">cols="75 75 150 50"</code> creates four columns that are 75 pixels, 75 pixels, 150 pixels, and 50 pixels wide, respectively. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="summary" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A summary of the tables contents.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The width of the table.  You can specify this as a number of pixels (<code type="inline">width="350px"</code>) or as a percentage of the width of the page (<code type="inline">width="75%"</code>).</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <tactic-code-urltactic> element                                                               -->
	<!-- ================================================================ -->
	<xsd:element name="tactic-code-urltactic" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Tactic code</title>
				<desc>Specify a tactic code for the urltactic code script</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- xM 2.1 egd 04 19 11:  Removed target-content-article (summary) element as it is no longer used in 6.0 -->
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <target-content-file> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="target-content-file">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Target content</title>
				<desc>The characteristics of, and links to, some target document, demo, presentation, code sample, etc. (as opposed to characteristics of *this* content).</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="note" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="filename" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The filename of the content.  Use the format xxxxx.yyy (include the file extension)</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="file-type" type="file-type" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>File type of the target content.  Ex: war, jsp, pdf, zip </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="size" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The file size of the target content.  <code type="inline">78 KB</code> is an example value.  Included in alt tag for download gif. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="target-content-type" type="content-type" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The type of the target content. Ex: technical article, demo, presentation, code sample, etc.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="file-description" type="file-description-50" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A short description (50 characters or less) of the target content.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-method-ftp" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the ftp transport protocol is available for the licensed download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="url-ftp" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The ftp URL of the target file.  Must include this, and/or url-http.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-method-http" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the http transport protocol is available for the licensed download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="url-http" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The http URL of the target file.  Must include this, and/or url-ftp.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-method-dd" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the Download Director method is available for the licensed download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="url-download-director" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The download-director URL of the target file.  Must include this, and/or url-http, and/or url-ftp.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="show-license" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Whether a license page should be displayed for the target file.  All code sample downloads require a license page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="url-license" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The URL of the license page, if there is one, that must be displayed for the target file.  All code sample downloads require a license page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="registration-required" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Whether the user must register in order to get the target file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <target-content-file-trial-program> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="target-content-file-trial-program">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Target content-trial-program</title>
				<desc>The download table for a trial program.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="operating-system" type="link-name"/>
				<xsd:element name="operating-system-description" type="link-name" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="url-registration" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>This is the registration URL for a trial program download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="version" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>This is the version, release, and modification level of the download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="size" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The core file size of the download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="max-size" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The file size of the download and all related components.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-method-ftp" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the ftp transport protocol is available for the licensed download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-method-http" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the http transport protocol is available for the licensed download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-method-dd" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the Download Director method is available for the licensed download.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <target-content-page> element                                  -->
	<!-- ================================================================ -->
	<xsd:element name="target-content-page">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title/>
				<desc>Used to define a link to a target page that contains a download, demo, presentation, code sample, etc.. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="note" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="target-content-type" type="content-type" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The type of the target content. Ex: white paper, article, demo, presentation, code sample, etc.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="link-text" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The keywords, each separated by a comma.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="url-target-page" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The URL for the target page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="tactic" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the stylesheet should append the onmouseover for urltactic. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="registration-required" type="yesno" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Whether the user must register to get the target file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <td> element                                                     -->
	<!-- ================================================================ -->
	<xsd:element name="td">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a table cell</title>
				<desc>Defines the contents of a table cell.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="dl"/>
				<xsd:element ref="em"/>
				<xsd:element ref="font"/>
				<xsd:element ref="forward-link-list"/>
				<xsd:element ref="i"/>
				<xsd:element ref="img"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<xsd:element ref="pre"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="span"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:element ref="table"/>
				<xsd:element ref="ul"/>
				<xsd:group ref="specialCharacters"/>
			     <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="bgcolor" type="xslfoColor">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code type="inline">bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%" summary="This table list the 16 color values defined in the XSL-FO standard">
								<tr>
									<td>aqua</td>
									<td>black</td>
									<td>blue</td>
									<td>fuchsia</td>
								</tr>
								<tr>
									<td>gray</td>
									<td>green</td>
									<td>lime</td>
									<td>maroon</td>
								</tr>
								<tr>
									<td>navy</td>
									<td>olive</td>
									<td>purple</td>
									<td>red</td>
								</tr>
								<tr>
									<td>silver</td>
									<td>teal</td>
									<td>white</td>
									<td>yellow</td>
								</tr>
							</table>
						</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="colspan" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the number of columns this cell should span. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS class to be applied to this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="headers" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The headers attribute applied to this cell.  Used to associate a data cell with one or more headings.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="rowspan" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the number of rows this cell should span.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="style" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS style to be applied to this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the width of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="valign">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
