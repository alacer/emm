				<xsd:element ref="forward-link-list"/>
				<xsd:element ref="heading"/>
				<xsd:element ref="img"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="span"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:element ref="table"/>
				<xsd:element ref="twisty-section"/>
