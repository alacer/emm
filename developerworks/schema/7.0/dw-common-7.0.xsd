	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <module-hidef-footer> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="module-hidef-footer">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>High Definition Footer Module</title>
				<desc>Defines a special footer module -- text and image or text only -- for the bottom of high definition landing pages.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="background-image-url" type="xsd:string" minOccurs="0"/>
