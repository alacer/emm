	<!-- NOT USED FOR 7.0 --> 
	<!-- ================================================================ -->
	<!-- <leadspace-split> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace-split">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace split</title>
				<desc>Defines a leadspace for a featured content module with two side-by-side images</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="leadspace-split-elements-group"/>
		</xsd:complexType>
	</xsd:element>	
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <left-nav-include> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="left-nav-include" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Specify a left navigation include</title>
				<desc>Specify an include with the appropriate left navigation for a page.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <li> element                                                     -->
	<!-- ================================================================ -->
	<xsd:element name="li">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an item in a list</title>
				<desc>Defines a list item for an ordered or unordered list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="dl"/>
				<xsd:element ref="em"/>
				<xsd:element ref="figure"/>
				<xsd:element ref="font"/>
				<xsd:element ref="i"/>
				<xsd:element ref="img"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<xsd:element ref="pre"/>
				<xsd:element ref="sidebar"/>
				<xsd:element ref="span"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:element ref="table"/>
				<xsd:element ref="ul"/>
				<xsd:group ref="specialCharacters"/>
			     <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="value" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>??? Is this the same as the HTML value attribute?  If so, this changes the item number for this item.  Subsequent list items are numbered accordingly as well. ???</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <link-block> element                                                                                            -->
	<!-- ================================================================ -->
	<xsd:element name="link-block">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link block.</title>
				<desc>Element defines a link block, which consists of a link and associated content, such as an image.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="link-block-elements-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <link-block-abstract> element                                                                                -->
	<!-- ================================================================ -->
	<xsd:element name="link-block-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Descriptive abstract for a module.</title>
				<desc>Descriptive abstract for a module that allows limited HTML tags for highlighting, simple formatting, special characters, and anchors.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="p"/>
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <link-list> element                                                                                            -->
	<!-- ================================================================ -->
	<xsd:element name="link-list">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link list.</title>
				<desc>Element defines a link list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="link-list-elements-group"/>
			</xsd:choice>
			<xsd:attribute name="style" type="link-list-style" default="bullet">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the type of link style used for link list items.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- Project Defiant jpp 10/06/11: Added link-list-categorized element -->
	<!-- USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <link-list-categorized> element        												                   				-->
	<!-- ================================================================ -->
	<xsd:element name="link-list-categorized">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Categorized link list</title>
				<desc>Defines the structure of a link list within a module that contains categorized links.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="link-list-item" type="link-categorized" maxOccurs="6"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <link-section> element        		         									                    			-->
	<!-- ================================================================ -->
	<xsd:element name="link-section">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link section</title>
				<desc>Defines a section of a column that can contain a link block or link list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="link-block"/>
				<xsd:element ref="link-list"/>
			</xsd:choice>
			<xsd:attribute name="separator" type="yesno" default="no">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether a separator should be added above the element</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <link-to-english> element                                           							 -->
	<!-- ================================================================ -->
	<xsd:element name="link-to-english" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link to English summary page.</title>
				<desc>For local site content, link to English summary page</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <meta-dcsubject> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="meta-dcsubject">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The DC.Subject meta tag.</title>
				<desc>The meta tag that defines the subject of this content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="cma-defined-subject" type="cma"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <meta-dctype> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="meta-dctype">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The DC.Type meta tag.</title>
				<desc>The meta tag that defines the content type of this content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="cma-defined-type" type="cma"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <meta-last-updated> element                                        -->
	<!-- ================================================================ -->
	<xsd:element name="meta-last-updated">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Meta last updated</title>
				<desc>Defines the HTML META tag that indicates when an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attributeGroup ref="dmy">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The day, month, and year (with leading zeros) an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attributeGroup>
			<xsd:attribute name="initials" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The initials (preferably at least three letters) of the editor who last changed this file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed moderator summary element as it's no longer used in 6.0 -->
