	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <pagegroup-overview-module> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="pagegroup-overview-module">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines pagegroup-overview element</title>
				<desc>The pagegroup-overview-module element defines an introductory module for each page in a landing pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="container-link-block" minOccurs="0"/>
