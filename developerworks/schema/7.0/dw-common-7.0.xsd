			<xsd:attribute name="display" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether the h1 title will be displayed on the page or will be hidden and used only for accessibility and search optimization.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>		
