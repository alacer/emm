					<xsd:element ref="pagegroup-overview" minOccurs="0"/>
					<xsd:element ref="pagegroup-overview-module" minOccurs="0"/>
				</xsd:choice>
				<!-- starting individual pages and subpages within pagegroup -->
				<xsd:element ref="page" minOccurs="2" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="content-space-primary-navigation" type="content-space-navigation-primary" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the type of primary navigation used by the pagegroup</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
