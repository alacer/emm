	<!-- xM 2.1 egd 04 15 11:  Removed objectvies and objective (summary) element as they are no longer used in 6.0 -->
	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- <ol> (ordered list) element                                      -->
	<!-- ================================================================ -->
	<xsd:element name="ol">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an ordered list</title>
				<desc>Defines an ordered list.  It contains some number of &lt;li&gt; items.  You can also nest ordered and unordered lists inside the list.</desc>
				<!--, which can in turn contain other lists, such as ordered lists, unordered lists, or definition lists.  In other words, an unordered list can't be a child of an ordered list; you must put the unordered list (at the second level) inside a list item.</desc> -->
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element ref="li"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="ul"/>
			</xsd:choice>
			<xsd:attribute name="start" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the starting number for this list.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the type of numbering that should be used for this list.  Valid values are:</desc>
						<ul>
							<li>
								<code type="inline">1</code> - Items are numbered with Arabic numerals (<code type="inline">1</code>, <code type="inline">2</code>, <code type="inline">3</code>, <code type="inline">4</code>, <code type="inline">5</code>, and so forth). This is the default numbering style.</li>
							<li>
								<code type="inline">A</code> - Items are numbered with uppercase alpha characters (<code type="inline">A</code>, <code type="inline">B</code>, <code type="inline">C</code>, <code type="inline">...</code>, <code type="inline">AA</code>, <code type="inline">AB</code>, <code type="inline">AC</code>, and so forth)</li>
							<li>
								<code type="inline">a</code> - Items are numbered with lowercase alpha characters (<code type="inline">a</code>, <code type="inline">b</code>, <code type="inline">c</code>, <code type="inline">...</code>, <code type="inline">aa</code>, <code type="inline">ab</code>, <code type="inline">ac</code>, and so forth)</li>
							<li>
								<code type="inline">I</code> - Items are numbered with uppercase Roman numerals (<code type="inline">I</code>, <code type="inline">II</code>, <code type="inline">III</code>, <code type="inline">IV</code>, <code type="inline">V</code>, and so forth)</li>
							<li>
								<code type="inline">i</code> - Items are numbered with lowercase Roman numerals (<code type="inline">i</code>, <code type="inline">ii</code>, <code type="inline">iii</code>, <code type="inline">iv</code>, <code type="inline">v</code>, and so forth)</li>
						</ul>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- NOT USED FOR 7.0 -->
