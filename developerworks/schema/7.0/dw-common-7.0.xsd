	<!-- NOT USED FOR 7.0 -->
	<!-- ================================================================ -->
	<!-- headingtype - what kind of heading is this?                      -->
	<!-- ================================================================ -->
	<!-- Can be a major or minor heading, figure heading, code heading, etc. -->
	<xsd:simpleType name="headingtype">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>The classification of a particular heading.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="code">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading (caption) for a code listing</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="figure">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading (caption) for a figure.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="hidef">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A major heading receiving special treatment on high definition landing pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="kp-step">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A major heading receiving special treatment on knowledge path pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>			
			<xsd:enumeration value="major">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A major heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>	
			<xsd:enumeration value="minor">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A minor heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="resource-list">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
