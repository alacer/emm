		<xsd:attribute name="highlight" type="yesno">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Indicates that an unlinked heading should be highlighted as new or important</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
