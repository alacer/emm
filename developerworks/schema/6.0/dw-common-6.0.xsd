			<xsd:enumeration value="IBM WebSphere Developer Technical Journal"/>
			<xsd:enumeration value="The Rational Edge"/>
			<xsd:enumeration value="IBM Database Magazine"/>
			<xsd:enumeration value="IBM Data Management magazine"/>
		    <xsd:enumeration value="IBM Business Process Management Journal "/>
			<xsd:enumeration value="IBM WebSphere Developer Technical Journal "/>
			<xsd:enumeration value="Rational Edge "/>
			<xsd:enumeration value="IBM Data Management magazine "/>
