	<!-- ================================================================ -->
	<!-- <title> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="title">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a document title</title>
				<desc>Title of the document. This will appear in content views and search results.</desc>
				<note>Titles must be unique for search.  Also used to create title tag, which is used in search results.</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
				<!-- 5.9 09/10/07 tdc:  Added alttoc element reference -->
				<xsd:element ref="alttoc" minOccurs="0"/>
			</xsd:choice>
			<xsd:attribute name="refname" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines an anchor for this heading.  You can use this attribute to reference this title from elsewhere in the document.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.4 2/2/06 fjc add refname -->
		</xsd:complexType>
	</xsd:element>
	<!-- 5.5 08/15/06  JPP/EGD Added title-tag element -->
	<!-- ================================================================ -->
	<!-- <title-tag> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="title-tag" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>title-tag</title>
				<desc>The title tag of a page or subpage in a multi-page document.  This is important for search and must be unique for each page/subpage. (The page title for each page/subpage is specified with the title element.)</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <tr> element                                                     -->
	<!-- ================================================================ -->
	<xsd:element name="tr">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a table row</title>
				<desc>Defines a row in a table.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<!-- 5.0 6/21 tdc:  Added th -->
				<xsd:element ref="th"/>
				<xsd:element ref="td"/>
			</xsd:choice>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the horizontal alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="bgcolor" type="xslfoColor">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code type="inline">bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%" summary="This table list the 16 color values defined in the XSL-FO standard">
								<tr>
									<td>aqua</td>
									<td>black</td>
									<td>blue</td>
									<td>fuchsia</td>
								</tr>
								<tr>
									<td>gray</td>
									<td>green</td>
									<td>lime</td>
									<td>maroon</td>
								</tr>
								<tr>
									<td>navy</td>
									<td>olive</td>
									<td>purple</td>
									<td>red</td>
								</tr>
								<tr>
									<td>silver</td>
									<td>teal</td>
									<td>white</td>
									<td>yellow</td>
								</tr>
							</table>
						</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS class to be applied to this row.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="valign">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.0 6/21 tdc:  Added class -->
		</xsd:complexType>
	</xsd:element>
	<!-- 4.0 4/20 tdc: Added per Gretchen.  ONLY TO SHOW UP IN PDFs, NOT HTML-->
	<!-- ================================================================ -->
	<!-- <trademark> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="trademark">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Trademark attribution</title>
				<desc>Trademark statements are used to give proper (legal) attribution to content displayed on dW with permission from the trademark owner.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
				<!-- <xsd:element ref="common-doc"/> -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 4.0 4/29 tdc: Added to group all trademarks together. -->
	<!-- ================================================================ -->
	<!-- <trademarks> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="trademarks">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Trademarks</title>
				<desc>Contains one or more trademark elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="heading" minOccurs="0"/>
				<xsd:element ref="trademark" maxOccurs="unbounded"/>
				<!-- Trademarks may appear under a heading or not -->
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.5 7/18 llk:  added translated by element per Russia request DR 1975-->
	<!-- ================================================================ -->
	<!-- <translator> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="translator">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Translated by</title>
				<desc>Define information about person/company who translated the content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0">
				<xsd:element name="translator-name" type="dw-person-name" minOccurs="0"/>
				<xsd:element ref="company-name" minOccurs="0"/>
			</xsd:choice>
			<xsd:attribute name="email" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The translator's email address.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="company-website" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The company's web site address.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/05/10 jpp/egd: Added twisty element -->
	<!-- ================================================================ -->
	<!-- <twisty> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty section</title>
				<desc>Defines a twisty section within a module</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="twisty-heading" type="link-name"/>
				<xsd:choice>
					<!-- 6.0 Maverick R3 05/18/10 jpp: Modified choice to use elements specific to twisty sections -->
					<xsd:sequence>
						<xsd:element name="twisty-abstract" minOccurs="0"/>
						<xsd:element ref="twisty-column" minOccurs="2" maxOccurs="3"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-html-body"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-level-two" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/18/10 jpp: Added twisty-abstract element -->
	<!-- ================================================================ -->
	<!-- <twisty-abstract> element                                                                                -->
	<!-- ================================================================ -->
	<xsd:element name="twisty-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty abstract</title>
				<desc>Descriptive abstract for a twisty section that allows limited HTML tags for highlighting, simple formatting, and special characters.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="em"/>
				<xsd:element ref="p"/>
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/18/10 jpp: Added twisty-column element -->
	<!-- ================================================================ -->
	<!-- <twisty-column> element        		         								        				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-column">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty column</title>
				<desc>Defines a column within a twisty section.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="link-list"/>
				<xsd:element ref="forward-link-list"/>				
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/05/10 jpp/egd: Added twisty-html-body element -->
	<!-- ================================================================ -->
	<!-- <twisty-html-body> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-html-body">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty HTML body</title>
				<desc>Defines an HTML body section for a twisty that supports free-form HTML tags</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<!-- 6.0 Maverick R3 03/03/10 jpp: Added forward-link and forward-link-list elements -->
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="twisty-body-elements-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/05/10 jpp/egd: Added twisty-level-two element -->
	<!-- ================================================================ -->
	<!-- <twisty-level-two> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-level-two">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Second level twisty</title>
				<desc>Defines second level twisty beneath a primary twisty</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="twisty-heading" type="link-name"/>
				<xsd:choice>
					<xsd:sequence>
						<xsd:element name="twisty-abstract" minOccurs="0"/>
						<xsd:element ref="twisty-column" minOccurs="2" maxOccurs="2"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-html-body"/>
					</xsd:sequence>
					<xsd:sequence>
						<xsd:element ref="twisty-level-three" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/06/10 jpp/egd: Added twisty-level-three element -->
	<!-- ================================================================ -->
	<!-- <twisty-level-three> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-level-three">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Third level twisty</title>
				<desc>Defines third level twisty beneath a secondary twisty</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="twisty-heading" type="link-name"/>
				<xsd:element ref="twisty-html-body"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 05/18/10 jpp: Added twisty-section element -->
	<!-- ================================================================ -->
	<!-- <twisty-section> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="twisty-section">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Twisty section</title>
				<desc>Allows a twisty section within the HTML container of a module or show/hide panel.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="1" maxOccurs="unbounded">
				<xsd:element ref="twisty"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <ul> (unordered list) element                                    -->
	<!-- ================================================================ -->
	<xsd:element name="ul">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an unordered list</title>
				<desc>Defines an unordered list.  The list contains one or more list items.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element ref="li"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="ul"/>
			</xsd:choice>
			<xsd:attribute name="type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The type of this unordered list.  ??? The HTML values are disc, circle, and square.  Are we supporting those here???</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
		<!-- Post xM (for V17) 11 09 11 egd: Added video element -->
	<!-- ================================================================ -->
	<!-- <video> element        	                        	                      												-->
	<!-- ================================================================ -->
	<xsd:element name="video">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Audio</title>
				<desc>Defines the video and transcript URLs for this content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="video-url" type="xsd:string" minOccurs="0"/>
			<xsd:element name="video-transcript-url" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>	
			<xsd:attributeGroup ref="itunes-feed-attributes"/>				
		</xsd:complexType>
	</xsd:element> 
	<!-- 5.7 03/21 egd Add element needed for auto-discovery of Web feeds by Google and others (DR 2265)-->
	<!-- ================================================================ -->
	<!-- <web-feed-autodiscovery> element                                    -->
	<!-- ================================================================ -->
	<xsd:element name="web-feed-autodiscovery">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a web feed autodiscovery tag (link rel)</title>
				<desc>Defines a web feed autodiscovery tag (link rel) so the Web feed can be discovered by Google, browsers, and others</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="feed-title" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The title of the Web feed.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="feed-url" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The URL of the Web feed.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="feed-type" type="webFeedType" default="rss">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The type of Web feed, RSS or Atom.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- ATTRIBUTE GROUPS  -->
	<!-- ================================================================ -->
	<!-- Day/Month/Year attributes                                        -->
	<!-- ================================================================ -->
	<!-- 4.0 06/10 tdc:  Had switched to gDay, gMonth, gYear.  But now adding leading zeros for day, month.  No longer using recentyear or updatedyear - don't need. -->
	<xsd:attributeGroup name="dmy">
		<xsd:attribute name="day" type="day">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The day of the month the article was created or updated.  Leading zeros are required.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="month" type="month" use="required">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The month of the year the article was created or updated.  Leading zeros are required.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="year" type="year" use="required">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The year the article was created or updated. </desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
		<!-- Post xM (for V17) 11 09 11 egd: Added itunes-feed-attributes group -->
	<!-- ================================================================ -->
	<!-- itunes-feed-attributes-group                                                                                          -->
	<!-- ================================================================ -->
	<xsd:attributeGroup name="itunes-feed-attributes">
		<xsd:attribute name="type" type="media-type">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The type of media for the iTunes feed.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="length" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The length in bytes of the audio or video for the iTunes feed</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="duration" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The duration of the audio or video in hours and minutes (HH:MM) for the iTunes feed </desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:attributeGroup>
	<!-- ================================================================ -->
	<!-- DATA TYPES  -->
	<!-- ================================================================ -->
	<!-- alignment - left, right, or center.                              -->
	<!-- ================================================================ -->
	<xsd:simpleType name="alignment">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="center">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Centers the item horizontally.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="left">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Left-aligns the item horizontally.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="right">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Right-aligns the item horizontally.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- auto - auto, yes, or no.                                         -->
	<!-- ================================================================ -->
	<xsd:simpleType name="auto">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="auto">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Determines the value automatically.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The value is set to <code type="inline">yes</code>.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="no">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The value is set to <code type="inline">no</code>.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 5.0 03/11/05 tdc:  Added category -->
	<!-- ================================================================ -->
	<!-- category - A category identifier                                                                                              -->
	<!-- ================================================================ -->
	<xsd:simpleType name="category">
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="collapse"/>
			<xsd:enumeration value="Learn"/>
			<xsd:enumeration value="Get products and technologies"/>
			<xsd:enumeration value="Discuss"/>
		</xsd:restriction>
		<!-- 5.11 09/06/08 tdc:  Collapse whitespace for all these values so won't get validation errors (DR 2667) -->
	</xsd:simpleType>
	<!-- xM R2 (R2.1) jpp 04/14/11: Added clickable-image type -->
	<!-- ================================================================ -->
	<!-- clickable-image complexType                                                                                             -->
	<!-- ================================================================ -->
	<xsd:complexType name="clickable-image" mixed="true">
		<xsd:attribute name="clickable" type="yesno" default="no">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Allows you specify whether the image should be clickable or not.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>		
	<!-- ================================================================ -->
	<!-- cma simpleType                                                  -->
	<!-- ================================================================ -->
	<!-- 5.5.1 10/18/06 tdc:  DR 1957:  Added cma simpleType -->
	<xsd:simpleType name="cma" xsi:nill="true">
		<xsd:restriction base="xsd:string">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<title>cma</title>
					<desc>An empty element, the presence of which indicates that the CMA should replace it with appropriate content before publication.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:restriction>
	</xsd:simpleType>
