					<xsd:documentation xml:lang="en">
						<desc>The author has not attained a dW author level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="contributing-author">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained the dW Contributing Author level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="professional-author">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained the dW Professional Author level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
						<xsd:enumeration value="master-author">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained a dW Master Author level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="master-author-2">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained a dW Master Author 2 level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="master-author-3">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained a dW Master Author 3 level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="master-author-4">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained a dW Master Author 4 level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="master-author-5">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author has attained a dW Master Author 5 level.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
