	<!-- 4.0 4/20 tdc: removed productname and productfamily -->
	<!-- 5.2 8/22/05 tdc:  Removed <related-list> element  -->
	<!-- ================================================================ -->
	<!-- <registration-url> element                                            -->
	<!-- ================================================================ -->
	<!-- 5.4 1/23/06 fjc added for tech and business briefing -->
	<xsd:element name="registration-url">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The location  and registration element for an event</title>
				<desc>The url to use for link to registration and location information view page</desc>
