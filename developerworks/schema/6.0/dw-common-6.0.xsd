	<!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
	<!-- 5.8 0421 egd: Added element for DC.Subject meta tag DR #2333 -->
	<!-- ================================================================ -->
	<!-- <meta-dcsubject> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="meta-dcsubject">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The DC.Subject meta tag.</title>
				<desc>The meta tag that defines the subject of this content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="cma-defined-subject" type="cma"/>
			</xsd:sequence>
		</xsd:complexType>
