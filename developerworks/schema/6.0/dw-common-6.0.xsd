	<!-- ================================================================ -->
	<!-- <link-abstract-optional>                                                                                        -->
	<!-- ================================================================ -->
	<xsd:complexType name="link-abstract-optional">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link Abstract Optional</title>
				<desc>Defines a link with an optional abstract within a section</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="text" type="link-name"/>
			<xsd:element name="url" type="xsd:string"/>
