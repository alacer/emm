	<!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
	<!-- ================================================================ -->
	<!-- <meta-last-updated> element                                        -->
	<!-- ================================================================ -->
	<xsd:element name="meta-last-updated">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Meta last updated</title>
				<desc>Defines the HTML META tag that indicates when an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attributeGroup ref="dmy">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The day, month, and year (with leading zeros) an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attributeGroup>
			<xsd:attribute name="initials" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The initials (preferably at least three letters) of the editor who last changed this file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed moderator summary element as it's no longer used in 6.0 -->
	<!-- 5.11 10/01/08 jpp/egd Added module element -->
	<!-- ================================================================ -->
	<!-- <module> element        											                                    			-->
	<!-- ================================================================ -->
	<xsd:element name="module">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Content module for a landing page.</title>
				<desc>Defines a content module within the main content area of a landing page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- 5.11 10/20/08 jpp: Added element to specify a tactic code to be applied to all module links unless the link appears to contain a tactic code -->
				<xsd:element name="module-tactic" type="link-name" minOccurs="0"/>
				<xsd:choice>
					<xsd:element ref="container-single"/>
					<!-- 6.0 Maverick R2 jpp-egd:  Added element for simple show/hide -->
					<xsd:element ref="container-single-show-hide" minOccurs="0" maxOccurs="2"/>
					<!-- 6.0 Maverick R2 jpp-egd:  Added element for multipanel show/hide -->
					<xsd:element ref="container-single-show-hide-multipanel" minOccurs="0" maxOccurs="2"/>
					<xsd:element ref="container" minOccurs="2" maxOccurs="2"/>
					<!-- 6.0 Maverick R2 jpp:  Added element container-html -->
					<xsd:element ref="container-html"/>					<!-- xM R2 (R2.3) jpp 06/14/11: Added include element -->
					<xsd:element ref="include"/>
					
				</xsd:choice>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 02-10-10 jpp: Added module-hidef-footer element -->
	<!-- ================================================================ -->
	<!-- <module-hidef-footer> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="module-hidef-footer">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>High Definition Footer Module</title>
				<desc>Defines a special footer module -- text and image or text only -- for the bottom of high definition landing pages.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="background-image-url" type="xsd:string" minOccurs="0"/>
					<xsd:choice>
						<xsd:element name="image-text-section" minOccurs="3" maxOccurs="3" type="hidef-footer-image-text-section-type"/>
						<xsd:element name="text-section" minOccurs="3" maxOccurs="5" type="hidef-footer-text-section-type"/>
					</xsd:choice>
				</xsd:sequence>
				<xsd:sequence>					
					<xsd:element name="hidef-footer-include" type="generic-right-box-include"/>
				</xsd:sequence>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/06/08 jpp/egd  Added module-high-visibility element -->
	<!-- ================================================================ -->
	<!-- <module-high-visibility> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="module-high-visibility">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>High Visibility Module</title>
				<desc>Defines a high visibility module -- text and image or image only -- for a landing page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- 6.0 Maverick R2 09/25/09 jpp: Changed the type reference from "link-name" to "module-heading" to add attributes required to link to the container heading -->
				<xsd:element name="container-heading" type="module-heading"/>
				<xsd:group ref="image-block-elements-group"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	<!-- 5.11 10/27/08 jpp/egd Added module-spotlight element -->
	<!-- ================================================================ -->
	<!-- <module-spotlight> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="module-spotlight">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Spotlight module for a landing page.</title>
				<desc>Defines a Spotlight module within the right column of a landing page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="right-box-section"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/06/08 jpp-egd: Added module-tabbed element -->
	<!-- ================================================================ -->
	<!-- <module-tabbed> element  		                                                                           	-->
	<!-- ================================================================ -->
	<xsd:element name="module-tabbed">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Tabbed module</title>
				<desc>Defines a module with tabs.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="accessibility-text" type="link-name"/>
				<xsd:element ref="module-tab" minOccurs="2" maxOccurs="5"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/06/08 jpp/egd Added module-tab element -->
	<!-- ================================================================ -->
	<!-- <module-tab> element        										                               				-->
	<!-- ================================================================ -->
	<xsd:element name="module-tab">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Module tab.</title>
				<desc>Defines a tab within a tabbed module.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="module-tab-name" type="link-name"/>
				<!-- 5.11 11/04/08 jpp:  Add new elements for more featured content and rss links for featured content tab -->
				<xsd:element name="more-featured-content" type="link" minOccurs="0"/>
				<xsd:element name="rss-link" type="link" minOccurs="0"/>
				<!-- 6.0 Maverick llk - added publish-schedule to enable japan pages to have separate publish schedule notices on each zone overview page-->
				<xsd:element name="publish-schedule" type="xsd:string" minOccurs="0"/>				
				<xsd:choice>
					<xsd:element ref="module-tab-container"/>
					<xsd:element ref="module-tab-show-hide"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="position" type="module-tab-position">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies the relative position of the tab within the tabbed module.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="custom-tab-name" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The name of a custom tab that requires special processing within the stylesheet.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.11 11/09/08 jpp: Added custom-tab-name element -->
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/28/08 jpp: Added module-tab-container element -->
	<!-- ================================================================ -->
	<!-- <module-tab-container> element        										               				-->
	<!-- ================================================================ -->
	<xsd:element name="module-tab-container">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Module tab container.</title>
				<desc>Defines the contents of a module tab.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="container-link-block" minOccurs="0"/>
				<xsd:element name="container-forward-link" type="link" minOccurs="0"/>
				<xsd:element ref="container-column" minOccurs="0" maxOccurs="3"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/28/08 jpp/egd Added module-tab-show-hide element -->
	<!-- ================================================================ -->
	<!-- <module-tab-show-hide> element        		         												-->
	<!-- ================================================================ -->
	<xsd:element name="module-tab-show-hide">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Module tab show/hide</title>
				<desc>Defines a tab with a show/hide feature.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="link-list"/>
			</xsd:choice>
			<!-- 6.0 Maverick R2 09/11/09 jpp/egd Added show-date attribute -->
			<xsd:attribute name="show-date" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the show/hide module should include a date.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <name> element    -->
	<!-- ================================================================ -->
	<xsd:element name="name" type="name-type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Author name</title>
				<desc>Defines the name of an individual author.  Will remain until investigation and resolution of how to handle the different parts of author name (prefix, firstname, middlename, lastname, suffix)</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- 5.4 04/21/06 tdc:  Added note element (originally for target-content-file or target-content-page notes) -->
	<!-- ================================================================ -->
	<!-- <note> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="note">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Note</title>
				<desc>Explanatory or reference text.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="p"/>				
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 15 11:  Removed objectvies and objective (summary) element as they are no longer used in 6.0 -->
	<!-- ================================================================ -->
	<!-- <ol> (ordered list) element                                      -->
	<!-- ================================================================ -->
	<xsd:element name="ol">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an ordered list</title>
				<desc>Defines an ordered list.  It contains some number of &lt;li&gt; items.  You can also nest ordered and unordered lists inside the list.</desc>
				<!--, which can in turn contain other lists, such as ordered lists, unordered lists, or definition lists.  In other words, an unordered list can't be a child of an ordered list; you must put the unordered list (at the second level) inside a list item.</desc> -->
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element ref="li"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="ul"/>
			</xsd:choice>
			<xsd:attribute name="start" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the starting number for this list.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the type of numbering that should be used for this list.  Valid values are:</desc>
						<ul>
							<li>
								<code type="inline">1</code> - Items are numbered with Arabic numerals (<code type="inline">1</code>, <code type="inline">2</code>, <code type="inline">3</code>, <code type="inline">4</code>, <code type="inline">5</code>, and so forth). This is the default numbering style.</li>
							<li>
								<code type="inline">A</code> - Items are numbered with uppercase alpha characters (<code type="inline">A</code>, <code type="inline">B</code>, <code type="inline">C</code>, <code type="inline">...</code>, <code type="inline">AA</code>, <code type="inline">AB</code>, <code type="inline">AC</code>, and so forth)</li>
							<li>
								<code type="inline">a</code> - Items are numbered with lowercase alpha characters (<code type="inline">a</code>, <code type="inline">b</code>, <code type="inline">c</code>, <code type="inline">...</code>, <code type="inline">aa</code>, <code type="inline">ab</code>, <code type="inline">ac</code>, and so forth)</li>
							<li>
								<code type="inline">I</code> - Items are numbered with uppercase Roman numerals (<code type="inline">I</code>, <code type="inline">II</code>, <code type="inline">III</code>, <code type="inline">IV</code>, <code type="inline">V</code>, and so forth)</li>
							<li>
								<code type="inline">i</code> - Items are numbered with lowercase Roman numerals (<code type="inline">i</code>, <code type="inline">ii</code>, <code type="inline">iii</code>, <code type="inline">iv</code>, <code type="inline">v</code>, and so forth)</li>
						</ul>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 11/07/08 jpp/egd Added option element -->
	<!-- ================================================================ -->
	<!-- <option> element        			                         										            	-->
	<!-- ================================================================ -->
	<xsd:element name="option">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Form option</title>
				<desc>Defines an option tag within a form.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:group ref="option-tag-group"/>
			</xsd:sequence>
			<xsd:attribute name="selected" type="yesno" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies if the option tag is selected</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="indent" type="yesno" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies if the option tag is selected</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <overview> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="overview">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>General intro text required in addition to abstract or abstract extended</title>
				<desc>General intro text required in addition to abstract or abstract extended</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Added overview-module element -->
	<!-- ================================================================ -->
	<!-- <overview-module> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="overview-module">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines overview-module element</title>
				<desc>The overview-module element defines an introductory module for a specific page in a landing pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="container-link-block" minOccurs="0"/>
				<xsd:element name="container-forward-link" type="link" minOccurs="0"/>
				<xsd:element ref="container-column" minOccurs="0" maxOccurs="3"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <p> (paragraph) element                                          -->
	<!-- ================================================================ -->
	<xsd:element name="p">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a paragraph</title>
				<desc>Defines a paragraph of text.  </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- Maverick 6.0 R2 jpp-egd 061509: Removing dl as a child of p.  Element is allowed within docbody. -->
				<!-- <xsd:element ref="dl"/> -->
				<!-- 5.11 10/20/08 jpp: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="font"/>
				<xsd:element ref="i"/>
				<xsd:element ref="img"/>
				<!-- Maverick 6.0 R2 jpp-egd 061509: Removing ol and sidebar as children of p.  Elements are allowed within docbody. -->
				<!-- <xsd:element ref="ol"/>
				<xsd:element ref="sidebar"/>  -->
				<!-- 5.7 03/19/07 jpp (DR #2252):  Added <span> tag -->
				<xsd:element ref="span"/>
				<!-- 5.11 10/20/08 jpp: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<!-- Maverick 6.0 R2 jpp-egd 061509: Removing table and ul as children of p.  Elements are allowed within docbody. -->
				<!-- <xsd:element ref="table"/>
				<xsd:element ref="ul"/> -->
				<xsd:group ref="specialCharacters"/>
				<!-- <font> is evil, must be killed -->
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the alignment for this paragraph.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.5 08/15/06 JPP/EGD Added page element -->
	<!-- ================================================================ -->
	<!-- <page> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="page">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines a page in a multi-page document.</title>
				<desc>Defines a page in a multi-page document like generic-landing-page-pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- start of elements used if there are no subpages -->
				<xsd:element ref="pageinfo" minOccurs="0"/>
				<xsd:element name="content" type="page-subpage-elements-type" minOccurs="0"/>
				<!-- end of elements used if there are no subpages -->
				<xsd:element ref="subpage" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="tab-position" type="tabposition" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify the relative position of the tab for this page in the primary navigation.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="content-space-secondary-navigation" type="content-space-navigation-secondary" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the type of secondary navigation used on the page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="output" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether an output file should be generated for this page link.  Default is 'yes'.  If an output file is not generated (output='no'), it is 
            assumed that the page is generated by an application or remote server.  While the page file may be generated by an application or remote server,
            it must be visually "persistent" with the other pages in the pagegroup.  The page file must look as if it were part of the pagegroup in all respects.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.7 03/21/07 jpp:  Added output attribute to specify whether a file should be generated for the page link (DR #2253) -->
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 12-03-09 JPP/EGD Added page-hidef element -->
	<!-- ================================================================ -->
	<!-- <page-hidef> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="page-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines a page in a high definition multi-page document.</title>
				<desc>Defines a page in a high definition multi-page document like generic-landing-pagegroup-hidef.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- start of elements used if there are no subpages -->
				<xsd:element ref="pageinfo" minOccurs="0"/>
				<xsd:element name="content" type="page-subpage-elements-hidef-type" minOccurs="0"/>
				<!-- end of elements used if there are no subpages -->
				<xsd:element ref="subpage-hidef" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="tab-position" type="tabposition" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify the relative position of the tab for this page in the primary navigation.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="content-space-secondary-navigation" type="content-space-navigation-secondary" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the type of secondary navigation used on the page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="output" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether content should be generated for this page link.  Default is 'yes'.  If content is not being generated (output='no'), it is 
            assumed that the page is generated by an application or remote server.  While the page file may be generated by an application or remote server,
            it must be visually "persistent" with the other pages in the pagegroup.  The referenced page must look as if it were part of the pagegroup in all respects.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 07/27/10 jpp: Added page-subtitle element -->
	<!-- ================================================================ -->
	<!-- <page-subtitle> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="page-subtitle">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a subtitle for a specific page within a landing pagegroup</title>
				<desc>Secondary title that may provide more information about the document.  When defined on a page, page-subtitle should override any subtitle definition made at the pagegroup level.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 07/27/10 jpp: Removed page-title element -->
	<!-- 6.0 Maverick R3 12/13/09 jpp: Added page-title-hidef element -->
	<!-- ================================================================ -->
	<!-- <page-title-hidef> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="page-title-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>High definition landing page title</title>
				<desc>Use to define the h1 title on a high definition page</desc>
				<note>Titles should be unique for search optimization.</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
			<xsd:attribute name="display" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether the h1 title will be displayed on the page or will be hidden and used only for accessibility and search optimization.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>		
	<!-- 5.5 08/15/06 JPP/EGD Added pagegroup element -->
	<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Updated pagegroup for v6.0 landing-generic-pagegroup and trial-program-pages -->
	<!-- ================================================================ -->
	<!-- <pagegroup> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="pagegroup">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines pagegroup characteristics</title>
				<desc>Defines the pagegroup characteristics in a multi-page document</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="id"/>
				<xsd:element ref="content-area-primary"/>
				<xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="content-area-subnav" minOccurs="0"/>
				<xsd:element ref="content-area-subnav-sublevel" minOccurs="0"/>
				<xsd:element ref="tactic-code-urltactic" minOccurs="0"/>
				<xsd:element ref="left-nav-include"/>
				<xsd:element ref="title"/>
				<!-- 6.0 Maverick R3 07-27-10 jpp: Restoring subtitle element under pagegroup -->
				<xsd:element ref="subtitle" minOccurs="0"/>
				<!-- 6.0 xM FIXPACK 09 21 11 egd Added rich-media-source element -->
				<xsd:element ref="rich-media-source" minOccurs="0"/>
				<xsd:element ref="meta-last-updated" minOccurs="0"/>
				<xsd:element ref="date-published"/>
				<xsd:element ref="date-updated" minOccurs="0"/>
				<xsd:element ref="rss-url" minOccurs="0"/>
				<xsd:element ref="brand-image" minOccurs="0"/>
				<xsd:element ref="customized-logo" minOccurs="0"/>
				<xsd:element ref="link-to-english" minOccurs="0"/>
				<xsd:choice>
					<xsd:element ref="pagegroup-overview" minOccurs="0"/>
					<xsd:element ref="pagegroup-overview-module" minOccurs="0"/>
				</xsd:choice>
				<!-- starting individual pages and subpages within pagegroup -->
				<xsd:element ref="page" minOccurs="2" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="content-space-primary-navigation" type="content-space-navigation-primary" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the type of primary navigation used by the pagegroup</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 12-03-09 jpp/egd: Added pagegroup-hidef element -->
	<!-- ================================================================ -->
	<!-- <pagegroup-hidef> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="pagegroup-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines pagegroup-hidef characteristics</title>
				<desc>Defines the pagegroup-hidef characteristics in a multi-page document</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="id"/>
				<xsd:element ref="content-area-primary"/>
				<xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="content-area-subnav" minOccurs="0"/>
				<xsd:element ref="content-area-subnav-sublevel" minOccurs="0"/>
				<xsd:element ref="tactic-code-urltactic" minOccurs="0"/>
				<!-- 6.0 Maverick R3 03-15-10 jpp: CMA application requires title element; restoring element for CMA use -->
				<!-- Title replaced by page-title-hidef element -->
				<xsd:element ref="title"/>		
				<!-- 6.0 xM FIXPACK 09 21 11 egd Added rich-media-source element -->
				<xsd:element ref="rich-media-source" minOccurs="0"/>		
				<xsd:element ref="meta-last-updated" minOccurs="0"/>
				<xsd:element ref="date-published"/>
				<xsd:element ref="date-updated" minOccurs="0"/>
				<!-- 6.0 Maverick R3 02-18-10 jpp: Added featured-content-module-hidef under pagegroup-hidef to define the leadspace at a pagegroup level -->
				<xsd:element ref="featured-content-module-hidef"/>				
				<!-- 6.0 Maverick R3 03-19-10 jpp: Added dw-wordmark element to allow wordmark to be turned off -->
				<xsd:element name="dw-wordmark" type="yesno" minOccurs="0"/>			
				<xsd:element ref="brand-image" minOccurs="0"/>
				<xsd:element ref="customized-logo" minOccurs="0"/>
				<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Make link-to-english element a pagegroup-level element; moved from page-subpage-elements-hidef-group -->
				<xsd:element ref="link-to-english" minOccurs="0"/>
				<!-- starting individual pages and subpages within pagegroup -->
				<xsd:element ref="page-hidef" minOccurs="2" maxOccurs="unbounded"/>
			</xsd:sequence>
			<!-- 6.0 Defiant R1 11/21/10 jpp: Added tabs attribute to pagegroup-hidef element -->
			<xsd:attribute name="tabs" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether tabs should be created for this pagegroup.  The 'no' option suppresses tabs in order to create a single high definition landing page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Added pagegroup-overview element -->
	<!-- ================================================================ -->
	<!-- <pagegroup-overview> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="pagegroup-overview">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines pagegroup-overview element</title>
				<desc>The pagegroup-overview element defines an introductory text area for each page in a landing pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0">
				<xsd:group ref="common-doc-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Added pagegroup-overview-module element -->
	<!-- ================================================================ -->
	<!-- <pagegroup-overview-module> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="pagegroup-overview-module">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines pagegroup-overview element</title>
				<desc>The pagegroup-overview-module element defines an introductory module for each page in a landing pagegroup.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="container-link-block" minOccurs="0"/>
				<xsd:element name="container-forward-link" type="link" minOccurs="0"/>
				<xsd:element ref="container-column" minOccurs="0" maxOccurs="3"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.5 08/15/06 JPP/EGD Added pageinfo -->
	<!-- ================================================================ -->
	<!-- <pageinfo> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="pageinfo">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Defines the pageinfo element.</title>
				<desc>Defines the name of the primary navigation item for this page and its associated URL.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="primary-nav-name" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the name of the primary navigation item for this page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="primary-nav-url" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Define the URL of the primary navigation item for this page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="page-end-btt-link" type="yesno" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify whether a back-to-top link should be automatically added to the bottom of the page.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.6 12/07/06 jpp:  Added the page-end-btt-link attribute to pageinfo (DR #2028) -->
		</xsd:complexType>
	</xsd:element>	
	<!-- 6.0 Maverick R3 05/05/10 jpp/egd: Added panel-html-body element -->
	<!-- ================================================================ -->
	<!-- <panel-html-body> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="panel-html-body">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Show/hide panel HTML body.</title>
				<desc>Allows free-form HTML elements within a show-hide-panel.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="br"/>
				<xsd:element ref="em"/>
				<xsd:element ref="figure"/>
				<xsd:element ref="forward-link-list"/>
				<xsd:element ref="heading"/>
				<xsd:element ref="img"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<xsd:element ref="strong"/>
				<xsd:element ref="span"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:element ref="table"/>
				<xsd:element ref="twisty-section"/>
				<xsd:element ref="ul"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <pdf> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="pdf">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a link to a PDF file</title>
				<desc>Link to printer-friendly PDF version of the document.</desc>
				<note>Only required when we have a mechanism to automatically create a PDF. Right now, some DD content has a printer-friendly version, though it is not a PDF; objective is for PDF to be used as the printer-friendly version for all documents.  PDF must be A4 and letter.  Will continue to do printer friendly for domains using it.  If non-open standards printable doc, put in the Resources section.  Who has other non-open types and what are they?</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="paperSize" type="paperSize">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The paper size used in the PDF file. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="size" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The size in KB of the PDF file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="url" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The URL of the PDF file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="pages" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The number of pages in the PDF file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.2 8/17/05 tdc:  Added pages attribute -->
			<!-- xM Fix Pack 09/20/11 jpp: Added track attribute to pdf element -->
			<xsd:attribute name="track" type="yesno" default="no">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Turn PDF tracking on.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.12 llk 03/2009: added poll element to support russian pages in maverick -->
	<!-- ================================================================ -->
	<!-- <poll-id> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="poll-id">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Associate a poll with this page</title>
				<desc>A popup poll associated with a particular dwhome page</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="lead-in-text" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Lead in text to be linked on from home page to generate the poll.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <pre> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="pre">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define preformatted content</title>
				<desc>Defines preformatted content.  All white space characters (spaces, newlines, tabs, and so forth) are preserved.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed prerequisites (summary) element as it is no longer used in 6.0 -->
	<!-- 5.3 12/19/05 jp:  Added definition for the product-technical-library-search element with scope attribute -->
	<!-- ================================================================ -->
	<!-- <product-technical-library-search> element   -->
	<!-- ================================================================ -->
	<xsd:element name="product-technical-library-search">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Product technical library search</title>
				<desc>Defines a product technical library search</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
			<xsd:attribute name="scope" type="scope-types">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the scope for the search.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 11/07/08 jpp/egd Added pulldown-form element -->
	<!-- ================================================================ -->
	<!-- <pulldown-form> element        			                         											-->
	<!-- ================================================================ -->
	<xsd:element name="pulldown-form">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Pulldown form</title>
				<desc>Defines the elements for a pulldown form.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="select-id" type="xsd:string"/>
