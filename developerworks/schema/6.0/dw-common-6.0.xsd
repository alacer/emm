	<!-- 5.3 11/30/05 JPP/EGD Added content-space-navigation-types  -->
	<!-- ====================================================================== -->
	<!-- content-space-navigation-types - The type of content space navigation being created  -->
	<!-- ====================================================================== -->
	<xsd:simpleType name="content-space-navigation-types">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="anchor-link-list-one-column">
				<xsd:annotation>
					<xsd:documentation>
						<desc>One-column anchor link list</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!-- 6.0 Maverick R2 09/24/09 jpp Added two and three column selection options -->
			<!-- 6.0 Maverick R3 03/06/10 jpp: Updated descriptions for two and three column selection options -->
			<xsd:enumeration value="anchor-link-list-two-column">
				<xsd:annotation>
					<xsd:documentation>
						<desc>Two-column anchor link list</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="anchor-link-list-three-column">
				<xsd:annotation>
					<xsd:documentation>
						<desc>Three-column anchor link list</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="none">
				<xsd:annotation>
					<xsd:documentation>
						<desc>No content space navigation</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!-- 5.3 12/20/05 jp:  Modified anchor link list value to specify number of columns  -->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 4.0 4/29 tdc: Renamed to be more generic so that we can use it in a group of attributes.  Added enumeration values, too. -->
	<!-- ================================================================ -->
	<!-- content-type - for code or some other type of content.     -->
	<!-- ================================================================ -->
	<xsd:simpleType name="content-type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>A type of content (different from the file type)</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="collapse"/>
			<xsd:enumeration value="Agenda"/>
			<xsd:enumeration value="Code sample"/>
			<xsd:enumeration value="Demo"/>
			<xsd:enumeration value="Podcast"/>
			<xsd:enumeration value="Presentation"/>
			<xsd:enumeration value="Product"/>
			<xsd:enumeration value="Product documentation"/>
			<xsd:enumeration value="Specification"/>
			<xsd:enumeration value="Technical article"/>
			<xsd:enumeration value="Transcript"/>
			<xsd:enumeration value="Whitepaper"/>
			<!-- 5.11 09/06/08 tdc:  Collapse whitespace for all these values so won't get validation errors (DR 2667) -->
			<!-- 5.5 fjc add Agenda -->
			<!-- 11/14/06 tdc:  Added Transcript -->
			<!-- 5.2 09/20 fjc: add podcast type -->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- USED FOR V6  -->
	<!-- 5.0 3/11 tdc:  Added contributor here as complex-type rather than element -->
	<!-- ================================================================ 
	contributor                           
	 ================================================================ -->
	<xsd:complexType name="contributor">
		<xsd:choice>
			<xsd:element name="cma-defined" type="cma"/>
			<xsd:sequence>
				<!-- 5.12 03/11/09 egd:  Added author level for author recognition gifs -->
				<xsd:element name="author-level" type="author-level-type" minOccurs="0"/>
				<xsd:element ref="bio" minOccurs="0"/>
				<xsd:element ref="img" minOccurs="0"/>
				<xsd:choice>
					<!-- post 5.11 llk - allow full name on tutorial summary pages -->
					<xsd:element ref="name" minOccurs="0"/>
					<xsd:element name="contributor-name" type="dw-person-name" minOccurs="0"/>
				</xsd:choice>
				<xsd:element ref="company-name" minOccurs="0"/>
			</xsd:sequence>
		</xsd:choice>
		<xsd:attribute name="email" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The email address. </desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="publish-email" type="yesno">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Lets editor choose whether or not to display the author's email address in the html.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="jobtitle" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>The contributor's job title.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="email-cc" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Allows someone on the dW team, like an acquisition editor, to be copied on contributor e-mails.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<!-- 5.5.1 10/11/06 tdc:  DR 1957:  Added choice of cma-defined. -->
		<!-- 5.5.1 9/27/06 keb:  Added publish-email attribute -->
	</xsd:complexType>
	<!-- 5.12 03/11/09 egd:  Added author-level-type -->
	<!-- ================================================================ -->
	<!-- author-level-type - the level of recognition this author has obtained                                   -->
	<!-- ================================================================ -->
	<!-- Maverick 6.0 R2 egd 03 14 10:  Remove master author 1 and miscellaneous description updates -->
		<xsd:simpleType name="author-level-type">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>Indicates the type of author level; i.e., how much author contribution to dW.</desc>
