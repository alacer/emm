	<!-- <hidef-footer-image-text-elements-group>                                                                              -->
	<!-- ================================================================ -->
	<xsd:group name="hidef-footer-image-text-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>hidef-footer-image-text-elements-group</title>
				<desc>Defines the group of elements for a high definition footer section that contains images and text.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="image-url" type="xsd:string"/>
			<xsd:element name="section-heading" type="heading-state"/>
			<xsd:element name="call-to-action-text" type="link-name-with-anchor"/>
			<xsd:element name="section-forward-link" type="link" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 6.0 Maverick R3 02-11-10 jpp: Added hidef-footer-text-elements-group -->
	<!-- ================================================================ -->
	<!-- <hidef-footer-text-elements-group>                                                                              -->
	<!-- ================================================================ -->
	<xsd:group name="hidef-footer-text-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>hidef-footer-text-elements-group</title>
				<desc>Defines the group of elements for a high definition footer section that text only.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="section-heading" type="heading-state"/>
			<xsd:element name="call-to-action-text" type="link-name-with-anchor"/>
			<xsd:element name="section-forward-link" type="link" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 10/06/08 jpp/egd Added image-block-elements group -->
	<!-- ================================================================ -->
	<!-- <image-block-elements-group>                                                                              -->
	<!-- ================================================================ -->
	<xsd:group name="image-block-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>image-block-elements-group</title>
				<desc>Defines the group of elements for a image link block.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- xM R2 (R2.1) jpp 04/14/11: Modified type for image-block-url element -->
			<xsd:element name="image-block-url" type="clickable-image"/>
			<xsd:element name="image-block-alt" type="xsd:string" minOccurs="0"/>
			<xsd:element name="image-block-heading" type="link-name" minOccurs="0"/>
			<!-- Maverick 6.0 R2 jpp-egd 061609: Changed from link-name to link-name-with-anchor to allow an anchor within the text -->
			<xsd:element name="call-to-action-text" type="link-name-with-anchor" minOccurs="0"/>
			<xsd:element name="image-block-link" type="link"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.12 llk 03/2009 Added image-list-combo-elements group -->
	<!-- 6.0 llk - added image-alt to address accessibility issues -->
	<!-- ================================================================ -->
	<!-- <image-list-combination-elements-group>                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="image-list-combination-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>image-list-combination-elements-group</title>
				<desc>Defines the group of elements for a featured content module using an image and list combination layout.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="target-url" type="xsd:string"/>
			<xsd:element name="image-url" type="xsd:string"/>
			<xsd:element name="image-alt" type="xsd:string"/>
			<xsd:element name="link-section"/>
			<xsd:element name="rss-link" type="link" minOccurs="0"/>
			<xsd:element name="background-color" type="xsd:string"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 10/03/08 jpp/egd Added image-text-combo-elements group -->
	<!-- ================================================================ -->
	<!-- <image-text-combination-elements-group>                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="image-text-combination-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>image-text-combination-elements-group</title>
				<desc>Defines the group of elements for a featured content module using an image and text combination layout.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="target-url" type="xsd:string"/>
			<xsd:element name="image-url" type="xsd:string"/>
			<!-- 6.0 Maverick R2 09/24/09 jpp: Added an optional alt tag element - image-alt - for images on landing pages -->
			<xsd:element name="image-alt" type="xsd:string" minOccurs="0"/>
			<xsd:element name="feature-heading" type="xsd:string"/>
			<xsd:element name="feature-abstract" type="link-name"/>
			<xsd:element name="background-color" type="xsd:string"/>
			<!-- 6.0 Maverick R2 10/02/09 jpp: Added optional call-to-action-text element for target-url link -->
			<xsd:element name="call-to-action-text" type="xsd:string" minOccurs="0"/>
			<!-- 6.0 Maverick R2 10/02/09 jpp: Added optional height element for image/text combination -->
			<xsd:element name="height" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Optionally, defines the height, in pixels, of the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- 6.0 Maverick R2 09/24/09 jpp: Added image-text-combination-static-elements group -->
	<!-- ================================================================ -->
	<!-- <image-text-combination-static-elements-group>                                                           -->
	<!-- ================================================================ -->
	<xsd:group name="image-text-combination-static-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>image-text-combination-static-elements-group</title>
				<desc>Defines the group of elements for a featured content module that uses a "static" image and text combination layout (no anchor links).</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="image-url" type="xsd:string"/>
			<xsd:element name="image-alt" type="xsd:string" minOccurs="0"/>
			<!-- 6.0 Maverick R2 10/02/09 jpp: Made height an optional element -->
			<xsd:element name="height" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="feature-heading" type="xsd:string"/>
			<xsd:element name="feature-abstract" type="link-name"/>
			<xsd:element name="background-color" type="xsd:string"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 11/07/08 jpp/egd Added input-tag group -->
	<!-- ================================================================ -->
	<!-- <input-tag-group>                                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="input-tag-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Input tag group</title>
				<desc>Defines the group of elements for an input tag used in a form.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string"/>
			<xsd:element name="type" type="xsd:string"/>
			<xsd:element name="value" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 10/03/08 jpp/egd Added image-text-combo-elements group -->
	<!-- ================================================================ -->
	<!-- <leadspace-elements-group>                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="leadspace-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>leadspace-elements-group</title>
				<desc>Defines the group of elements for the featured content module using a leadspace image.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="image-url" type="xsd:string"/>
			<xsd:element name="image-alt" type="xsd:string"/>
			<xsd:element name="target-url" type="xsd:string"/>
			<!-- 6.0 Maverick R2 10/02/09 jpp: Added optional height element for leadspace image -->
			<xsd:element name="height" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Optionally, defines the height, in pixels, of the leadspace image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- 6.0 Maverick R3 02/15/10 jpp:  Added leadspace-overlay-elements-group -->
	<!-- ================================================================ -->
	<!-- <leadspace-overlay-elements-group>                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="leadspace-overlay-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>leadspace-overlay-elements-group</title>
				<desc>Defines the group of elements for a featured content module using a leadspace with an overlay.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>			
			<xsd:element name="image-url" type="xsd:string"/>
			<xsd:element name="height" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="image-alt" type="xsd:string" minOccurs="0"/>
			<xsd:element name="overlay-navigation-link-first" type="link"/>
			<xsd:element name="overlay-navigation-link-second" type="link" minOccurs="0"/>
			<xsd:element name="overlay-navigation-link-third" type="link" minOccurs="0"/>			
			<xsd:element name="overlay-heading" type="xsd:string" minOccurs="0"/>
			<xsd:element name="overlay-subtitle" type="xsd:string" minOccurs="0"/>
			<xsd:element name="overlay-forward-link" type="link" minOccurs="0" maxOccurs="2"/>
		</xsd:sequence>
	</xsd:group>
	<!-- xM R2 (R2.3) jpp 07/14/11: Added leadspace-split-elements-group -->
	<!-- ================================================================ -->
	<!-- <leadspace-split-elements-group>                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="leadspace-split-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>leadspace-split-elements-group</title>
				<desc>Defines the group of elements for the featured content module using a leadspace-split format.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="image-first-url" type="xsd:string"/>
			<xsd:element name="image-first-alt" type="xsd:string"/>
			<xsd:element name="image-first-target-url" type="xsd:string"/>
			<xsd:element name="image-second-url" type="xsd:string"/>
			<xsd:element name="image-second-alt" type="xsd:string"/>
			<xsd:element name="image-second-target-url" type="xsd:string"/>
			<xsd:element name="height" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Optionally, defines the height, in pixels, of the leadspace images.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>	
	<!-- 5.11 10/03/08 jpp/egd Added link-block-elements group -->
	<!-- ================================================================ -->
	<!-- <link-block-elements-group>                                                                              -->
	<!-- ================================================================ -->
	<xsd:group name="link-block-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>link-block-elements-group</title>
				<desc>Defines the group of elements for a module link block.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="link-block-heading" type="heading-state" minOccurs="0"/>
			<xsd:element ref="link-block-abstract"/>
			<xsd:element name="link-block-image-url" type="xsd:string" minOccurs="0"/>
			<!-- 5.11 10/08/08 jpp/egd Added forward link inside of a link block -->
			<xsd:element name="link-block-forward-link" type="link" minOccurs="0"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 10/03/08 jpp/egd Added link-list-elements group -->
	<!-- ================================================================ -->
	<!-- <link-list-elements-group>                                                                              -->
	<!-- ================================================================ -->
	<xsd:group name="link-list-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>link-list-elements-group</title>
				<desc>Defines the group of elements for a module link list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="link-list-heading" type="heading-state" minOccurs="0"/>
			<xsd:element name="link-list-item" type="link-abstract-optional" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 11/07/08 jpp/egd Added option-tag group -->
	<!-- ================================================================ -->
	<!-- <option-tag-group>                                                                                   -->
	<!-- ================================================================ -->
	<xsd:group name="option-tag-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Option tag group</title>
				<desc>Defines the group of elements for an option tag used in a form.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- <xsd:element name="indent" type="yesno" minOccurs="0" maxOccurs="1"/> -->
			<xsd:element name="text" type="xsd:string"/>
			<xsd:element name="value" type="xsd:string"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.5 08/15/06 JPP/EGD Added page-subpage-elements-group -->
	<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Updated page-subpage-elements-group for v6.0 landing-generic-pagegroup and trial-program-pages -->
	<!-- ================================================================ -->
	<!-- <page-subpage-elements-group>                                                -->
	<!-- ================================================================ -->
	<xsd:group name="page-subpage-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Elements used to create a page or subpage in a multipage document.</title>
				<desc>Defines the elements used to create a page or subpage in a multipage document.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="meta-information" type="page-subpage-meta-type" minOccurs="0"/>
			<!-- 6.0 Maverick R3 07-27-10 jpp: Removed page-title element at the page level; not allowed per ibm.com standard -->
			<!-- 6.0 Maverick R3 07-27-10 jpp: Changed subtitle element to page-subtitle element.  Allows editor to override global subtitle element 
				at the page level in special cases (example: trial program pages) -->
			<xsd:element ref="page-subtitle" minOccurs="0"/>
			<xsd:element ref="featured-content-module" minOccurs="0"/>
			<xsd:choice>
				<xsd:element ref="overview" minOccurs="0"/>
				<xsd:element ref="overview-module" minOccurs="0"/>
			</xsd:choice>
			<xsd:element ref="selected-tab-container"/>
			<!-- 6.0 Maverick R3 07-15-10 jpp/egd: The editorial team has agreed that the following two trial page elements can be removed for v6.0; here for reference only --> 
			<!-- <xsd:element name="registration-enrollment-url" type="xsd:string" minOccurs="0"/> -->
			<!-- <xsd:element ref="target-content-file-trial-program" minOccurs="0" maxOccurs="unbounded"/> -->
			<xsd:element ref="module" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="target-content-file" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="contact-module-include" type="yesno" default="no" minOccurs="0"/>
			<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Updated ready-to-buy element to allow multiple products -->
			<xsd:element name="ready-to-buy" type="link" minOccurs="0" maxOccurs="3"/>
			<xsd:element ref="module-high-visibility" minOccurs="0" maxOccurs="3"/>
			<xsd:element name="top-right-include" type="generic-right-box-include" minOccurs="0" maxOccurs="4"/>
			<xsd:element ref="right-box" minOccurs="0" maxOccurs="4"/>
			<xsd:element name="bottom-right-include" type="generic-right-box-include" minOccurs="0" maxOccurs="4"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 6.0 Maverick R3 12/04/09 JPP/EGD Added page-subpage-elements-hidef-group -->
	<!-- ================================================================ -->
	<!-- <page-subpage-elements-hidef-group>                                                -->
	<!-- ================================================================ -->
	<xsd:group name="page-subpage-elements-hidef-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Elements used to create a page or subpage in a multipage hidef document.</title>
				<desc>Defines the elements used to create a page or subpage in a multipage hidef document.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="meta-information" type="page-subpage-meta-type" minOccurs="0"/>
			<!-- 6.0 Maverick R3 02-18-10 jpp: Made featured-content-module-hidef element optional when it appears in page-subpage-elements-hidef-group -->
			<xsd:element ref="featured-content-module-hidef" minOccurs="0"/>
			<xsd:element ref="page-title-hidef"/>
			<xsd:element ref="abstract-extended" minOccurs="0"/>
			<!-- Replaced overview with abstract-extended: <xsd:element ref="overview" minOccurs="0"/> -->
			<xsd:element ref="module-tabbed" minOccurs="0"/>
			<xsd:element ref="module" maxOccurs="unbounded"/>
			<!-- Right column -->	
			<xsd:element ref="brand-image" minOccurs="0"/>
			<xsd:element ref="customized-logo" minOccurs="0"/>
			<xsd:element name="contact-module-include" type="yesno" default="no" minOccurs="0"/>
			<xsd:element ref="module-high-visibility" minOccurs="0" maxOccurs="3"/>
			<xsd:element name="top-right-include" type="generic-right-box-include" minOccurs="0" maxOccurs="4"/>
			<xsd:element ref="right-box" minOccurs="0" maxOccurs="4"/>
			<xsd:element name="bottom-right-include" type="generic-right-box-include" minOccurs="0" maxOccurs="4"/>
			<!-- 6.0 Maverick R3 02-10-10 jpp: Added module-hidef-footer element to page-subpage-elements-hidef-group -->
			<xsd:element ref="module-hidef-footer" minOccurs="0"/>
			<!-- 6.0 Maverick R3 07-15-10 jpp/egd: Moved link-to-english element to pagegroup-hidef element -->
		</xsd:sequence>
	</xsd:group>
	<!-- 5.5 08/15/06 JPP/EGD Added page-subpage-meta-group -->
	<!-- ================================================================ -->
	<!-- <page-subpage-meta-group>                                                -->
	<!-- ================================================================ -->
	<xsd:group name="page-subpage-meta-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Title tag and meta tags for a page or subpage.</title>
				<desc>Defines the title tag and any meta tags for a page or subpage that should be unique for search.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- 5.6 12/05/06 jpp:  Updated title-tag to reference the title-tag element (DR # 2150) -->
			<xsd:element ref="title-tag"/>
			<xsd:element name="keywords" type="xsd:string"/>
			<!-- 5.10 egd 02/29: added meta-dctype as an optional element DR 2788-->
			<xsd:element ref="meta-dctype" minOccurs="0"/>
			<!-- 5.8 egd 04/21: added meta-dcsubject as an optional element DR 2333 -->
			<xsd:element ref="meta-dcsubject" minOccurs="0"/>
			<xsd:element name="abstract" type="xsd:string"/>
			<!-- 5.7 03/24 egd: Added web feed autodiscovery (DR 2276)-->
			<xsd:element ref="web-feed-autodiscovery" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:group>
