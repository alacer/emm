	<!-- 6.0 Maverick R2 09/25/09 jpp: Added module-heading complexType -->
	<!-- ================================================================ -->
	<!-- <module-heading>                                                                                          -->
	<!-- ================================================================ -->
	<xsd:complexType name="module-heading" mixed="true">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Module heading</title>
				<desc>Defines the heading for a content module</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:group ref="specialCharacters"/>
		</xsd:choice>
		<xsd:attribute name="alttoc" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Defines alternate text to be used in the table of contents.  This attribute is useful when the heading has lots of text.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="name" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Names this heading.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="refname" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Defines an ID for this heading.  You can use this attribute to reference this heading from elsewhere in the document.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="back-to-top" type="yesno" use="optional">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Indicates whether the stylesheet should add a Back to top link at the end of the associated module.</desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>	
