			<xsd:documentation xml:lang="en">
				<title>link</title>
				<desc>Defines a link within a section</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="text" type="link-name"/>
			<xsd:element name="url" type="xsd:string"/>
		</xsd:sequence>
		<xsd:attribute name="tactic" type="yesno">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Defines whether the stylesheet should append the onmouseover for urltactic. </desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
			<!-- 6.0 maverick llk add new as attribute to link element; only used by Japan -->
			<xsd:attribute name="new" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates that this content is new to the site; used by Japan only. </desc>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
		<!-- 5.7 03/13 egd Added attribute for urltactic URLs DR #2140 -->
		<!-- 5.7 04/02 egd removed default=yes -->
	</xsd:complexType>
	<!-- 5.1 06/23/05 JPP/EGD Added page-section  -->
	<!-- ================================================================ -->
	<!-- <link-abstract>                                               -->
	<!-- ================================================================ -->
	<xsd:complexType name="link-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>link</title>
				<desc>Defines a link with an abstract within a section</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="text" type="link-name"/>
			<xsd:element name="url" type="xsd:string"/>
			<xsd:element name="abstract-text" type="link-name"/>
		</xsd:sequence>
		<xsd:attribute name="tactic" type="yesno">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Defines whether the stylesheet should append the onmouseover for urltactic. </desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<!-- 6.0 maverick llk add new as attribute to link element; only used by Japan -->
		<xsd:attribute name="new" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates that this content is new to the site; used by Japan only. </desc>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
		<!-- 5.7 03/13 egd Added attribute for urltactic URLs DR #2140 -->
		<!-- 5.7 04/02 egd removed default=yes -->
