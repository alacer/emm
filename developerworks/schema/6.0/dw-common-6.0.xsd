	<!-- 4.0 4/19 tdc: renamed to resource-list -->
	<!-- ================================================================ -->
	<!-- <resource-list> element                                        													   -->
	<!-- ================================================================ -->
	<xsd:element name="resource-list">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a list of resources</title>
				<desc>Defines a list of resources related to this article.  </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element ref="dl"/>
				<xsd:element ref="heading"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<xsd:element ref="ul"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <resources> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="resources">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of document resources</title>
				<desc>Collection of References to  resources</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="resource" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added rich-media element -->
	<!-- ================================================================ -->
	<!-- <rich-media> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="rich-media">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Rich media</title>
				<desc>Defines a featured content module using rich media.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<!-- 6.0 Maverick R3 12-15-09 jpp: Added a choice statement to allow an include file to be used to embed rich media components -->
			<xsd:choice>
				<xsd:sequence>
					<xsd:element ref="include"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:group ref="leadspace-elements-group"/>
					<!-- DO WE NEED TO ALLOW EDITORS TO SELECT FILE TYPE -->
					<xsd:element name="rich-media-filename" type="xsd:string"/>
