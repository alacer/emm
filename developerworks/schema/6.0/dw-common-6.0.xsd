	<!-- page-types - The type of landing page being created                -->
	<!-- ================================================================ -->
	<xsd:simpleType name="page-types">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="product">
				<xsd:annotation>
					<xsd:documentation>
						<desc>A landing page for a product</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="product-condensed">
				<xsd:annotation>
					<xsd:documentation>
						<desc>A product landing page that is not frequently updated with new content so it doesn't usually have a Latest content section. It also may not contain other traditional sections like downloads and community.</desc>
