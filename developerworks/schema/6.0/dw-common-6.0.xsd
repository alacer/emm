            <xsd:attribute name="link" type="yesno" default="yes" use="optional">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">
                        <desc>Specifies whether a hypertext link should be generated for
                            this cross-reference.</desc>
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:attribute>
        </xsd:complexType>
</xsd:element>
