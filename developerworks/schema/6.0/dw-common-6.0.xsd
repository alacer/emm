	<xsd:complexType name="hidef-footer-text-section-type">
		<xsd:group ref="hidef-footer-text-elements-group"/>
	</xsd:complexType>
	<!-- ================================================================ -->
	<!-- inline - should an item be inline or a separate section?         -->
	<!-- ================================================================ -->
	<xsd:simpleType name="inline">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="inline">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>This item should be formatted inline.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="section">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>This item should be formatted as a separate section.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
