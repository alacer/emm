	<!-- ================================================================ -->
	<!-- headingtype - what kind of heading is this?                      -->
	<!-- ================================================================ -->
	<!-- Can be a major or minor heading, figure heading, code heading, etc. -->
	<xsd:simpleType name="headingtype">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>The classification of a particular heading.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="code">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading (caption) for a code listing</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="figure">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading (caption) for a figure.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!-- 6.0 Maverick R3 03-03-10 jpp: Added hidef heading type to enable special treatment of major headings on high definition landing pages -->
			<xsd:enumeration value="hidef">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A major heading receiving special treatment on high definition landing pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		    <!-- 6.0 xM r2.3 06/20/11 tdc: Added kp-step heading type for knowledge path step headings -->
			<xsd:enumeration value="kp-step">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A major heading receiving special treatment on knowledge path pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>			
			<xsd:enumeration value="major">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A major heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>	
			<xsd:enumeration value="minor">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A minor heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="resource-list">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The heading for the list of related resources.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="rightbox">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading for a right column text box.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="sidebar">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading for a sidebar.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="table">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>A heading for a table.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!-- 5.3 12/15/05 jpp-egd:  Added heading type for right column boxes -->
			<!-- 5.0 tdc:  Added table type -->
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 5.11 10/03/08 jpp/egd Added heading-state complexType -->
	<!-- ================================================================ -->
	<!-- heading-state                                                                                                    -->
	<!-- ================================================================ -->
	<xsd:complexType name="heading-state">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>heading-state</title>
				<desc>Defines a heading within a module that may or may not be 'linkable'.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="text" type="link-name"/>
			<xsd:element name="url" type="xsd:string" minOccurs="0"/>
