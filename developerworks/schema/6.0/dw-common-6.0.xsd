			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the horizontal alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="bgcolor" type="xslfoColor">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code type="inline">bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%" summary="This table list the 16 color values defined in the XSL-FO standard">
								<tr>
									<td>aqua</td>
									<td>black</td>
									<td>blue</td>
									<td>fuchsia</td>
								</tr>
								<tr>
									<td>gray</td>
									<td>green</td>
									<td>lime</td>
									<td>maroon</td>
								</tr>
								<tr>
									<td>navy</td>
									<td>olive</td>
									<td>purple</td>
									<td>red</td>
								</tr>
								<tr>
									<td>silver</td>
									<td>teal</td>
									<td>white</td>
									<td>yellow</td>
								</tr>
							</table>
						</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="valign" type="vertical">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical alignment of the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
