	<!-- 5.2 8/22/05 tdc:  Removed subscription-level (dWS) -->
	<!-- 5.1 05/31/05 JPP/EGD Added top-story -->
	<!-- ================================================================ -->
	<!-- <top-story> type                                                 -->
	<!-- ================================================================ -->
	<xsd:complexType name="top-story">
		<xsd:group ref="common-top-story-group"/>
		<xsd:attribute name="tactic" type="yesno">
			<xsd:annotation>
				<xsd:documentation xml:lang="en">
					<desc>Defines whether the stylesheet should append the onmouseover for urltactic. </desc>
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<!-- 5.7 03/17 egd Added attribute for urltactic URLs DR #2140 -->
		<!-- 5.7 04/02 egd removed default=yes -->
	</xsd:complexType>
	<!-- Post xM (for V17) 11 09 11 egd: Removed complexType urls-audio -->
	<!-- Post xM (for V17) 11 09 11 egd: Removed complexType urls-video -->	
	<!-- ================================================================ -->
	<!-- vertical - vertical alignment values                             -->
	<!-- ================================================================ -->
	<xsd:simpleType name="vertical">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>The vertical alignment of the content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="bottom">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Vertically aligns the content along the bottom.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="middle">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Vertically aligns the content in the middle.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="top">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Vertically aligns the content at the top.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- 5.7 03/21 egd Added webFeedType  -->
	<!-- ================================================================ -->
	<!-- webFeedType - now two kinds, rss and atom                                      -->
	<!-- ================================================================ -->
	<xsd:simpleType name="webFeedType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="rss"/>
			<xsd:enumeration value="atom"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- xslfoColor - one of the 16 color names from XSL-FO.              -->
	<!-- ================================================================ -->
	<xsd:simpleType name="xslfoColor">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>One of the sixteen color names defined in the XSL-FO standard, or a hexadecimal color code, as in <code type="inline">#FF00FF</code> or <code type="inline">FF00FF</code>.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="aqua"/>
			<xsd:pattern value="black"/>
			<xsd:pattern value="blue"/>
			<xsd:pattern value="gray"/>
			<xsd:pattern value="fuchsia"/>
			<xsd:pattern value="gray"/>
			<xsd:pattern value="green"/>
			<xsd:pattern value="lime"/>
			<xsd:pattern value="maroon"/>
			<xsd:pattern value="navy"/>
			<xsd:pattern value="olive"/>
			<xsd:pattern value="purple"/>
			<xsd:pattern value="red"/>
			<xsd:pattern value="silver"/>
			<xsd:pattern value="teal"/>
			<xsd:pattern value="white"/>
			<xsd:pattern value="yellow"/>
			<xsd:pattern value="[#]?[A-Fa-f0-9]{6}"/>
		</xsd:restriction>
		<!-- 4/7/03 Doug changed the colors to allow names and hex -->
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- yesno - a simple yes or no.                                      -->
	<!-- ================================================================ -->
	<xsd:simpleType name="yesno">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="yes"/>
			<xsd:enumeration value="no"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- year - the value for year must be reasonable               -->
	<!-- ================================================================ -->
	<!-- 6.0 xM R1 12/20/10 jpp: Updated maxInclusive value for year to 2020 -->	
	<xsd:simpleType name="year">	
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:minInclusive value="1996"/>
			<xsd:maxInclusive value="2020"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- zeroOrOne - either 0 or 1.                                       -->
	<!-- ================================================================ -->
	<xsd:simpleType name="zeroOrOne">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>Whether this item has a border.  A value of <code type="inline">0</code> means no border, <code type="inline">1</code> means there will be one.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================================ -->
	<!-- ELEMENT GROUPS -->
	<!-- ================================================================ -->
	<!-- ================================================================ -->
	<!-- <common-doc-group>                                                -->
	<!-- ================================================================ -->
	<!-- 5.3 12/15/05 jpp-egd:  added include -->
	<xsd:group name="common-doc-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the content of a document</title>
				<desc>Defines the content of a document.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element ref="a"/>
			<xsd:element ref="b"/>
			<xsd:element ref="blockquote"/>
			<xsd:element ref="br"/>
			<xsd:element ref="code"/>
			<xsd:element ref="dl"/>
			<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
			<xsd:element ref="em"/>
			<xsd:element ref="figure"/>
			<xsd:element ref="heading"/>
			<xsd:element ref="i"/>
			<xsd:element ref="img"/>
			<xsd:element ref="include"/>
			<xsd:element ref="ol"/>
			<xsd:element ref="p"/>
			<xsd:element ref="pre"/>
			<xsd:element ref="sidebar"/>
			<!-- xM R2 (R2.3) jpp 08/02/11: Added sidebar-custom element to common-doc-group -->
			<xsd:element ref="sidebar-custom"/>
			<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
			<xsd:element ref="strong"/>
			<!-- 5.7 03/19/07 jpp (DR #2252):  Added <span> tag -->
			<xsd:element ref="span"/>
			<xsd:element ref="sub"/>
			<xsd:element ref="sup"/>
			<xsd:element ref="table"/>
			<xsd:element ref="ul"/>
			<xsd:group ref="specialCharacters"/>
		</xsd:choice>
	</xsd:group>
	<!-- 5.1 05/31/05 JPP/EGD Added common-top-story-group -->
	<!-- ================================================================ -->
	<!-- <common-top-story-group>                                                -->
	<!-- ================================================================ -->
	<xsd:group name="common-top-story-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>common-top-story-group</title>
				<desc>Defines the group of elements for the feature story area of the page</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="target-file-url" type="xsd:string"/>
			<xsd:element name="main-image-url" type="xsd:string"/>
			<xsd:element name="top-story-heading" type="xsd:string"/>
			<xsd:element name="top-story-abstract" type="link-name"/>
		</xsd:sequence>
	</xsd:group>
	<!-- 5.11 10/20/08 jpp: Added container-link-block-elements group -->
	<!-- ================================================================ -->
	<!-- <container-link-block-elements-group>                                                                  -->
	<!-- ================================================================ -->
	<xsd:group name="container-link-block-elements-group">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>link-block-elements-group</title>
				<desc>Defines the group of elements for a container link block.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<!-- 5.11 11/07/08 jpp/egd : Added form element -->
			<xsd:element ref="form" minOccurs="0" maxOccurs="2"/>
			<xsd:sequence>
				<xsd:element name="link-block-heading" type="heading-state" minOccurs="0"/>
				<xsd:element ref="link-block-abstract"/>
				<xsd:element name="link-block-image-url" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
		</xsd:choice>
