	<!-- 5.11 10/01/08 jpp/egd Added leadspace element -->
	<!-- ================================================================ -->
	<!-- <leadspace> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace</title>
				<desc>Defines a leadspace image for a featured content module.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="leadspace-elements-group"/>
			<!-- 6.0 Maverick R2 09/24/09 jpp: Added multicolumn attribute -->
			<xsd:attribute name="multicolumn" type="yesno" default="no" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify a multicolumn leadspace image</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 09/24/09 jpp: Added leadspace-decorative element -->
	<!-- ================================================================ -->
	<!-- <leadspace-decorative> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace-decorative">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace decorative</title>
				<desc>Defines a leadspace image for the featured content module that is for decorative use only and has no anchor link.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="image-url" type="xsd:string"/>
				<xsd:element name="image-alt" type="xsd:string" minOccurs="0"/>
				<xsd:element name="height" type="xsd:integer" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation xml:lang="en">
							<desc>Optionally, defines the height, in pixels, of the leadspace image.</desc>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="multicolumn" type="yesno" default="no" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify a multicolumn leadspace image</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/31/08 jpp: Added leadspace-overlay element -->
	<!-- ================================================================ -->
	<!-- <leadspace-overlay> element            		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace-overlay">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<!-- 6.0 Maverick R2 09/24/09 jpp: Corrected the title -->
				<title>Leadspace overlay</title>
				<desc>Defines a format for a featured content module that has a leadspace with overlayed text.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<!-- 6.0 Maverick R3 02/15/10 jpp:  Changed group reference for leadspace-overlay element to leadspace-overlay-elements-group -->
		<xsd:complexType>
			<xsd:group ref="leadspace-overlay-elements-group"/>
			<!-- 6.0 Maverick R3 02/15/10 jpp:  Added attribute to select overlay text color -->
			<xsd:attribute name="overlay-text-color" type="overlaycolor" default="white">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Allows user to select overlay text colors supported by ibm.com standard</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- xM R2 (R2.3) jpp 07/14/11: Added leadspace-split element --> 
	<!-- ================================================================ -->
	<!-- <leadspace-split> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="leadspace-split">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Leadspace split</title>
				<desc>Defines a leadspace for a featured content module with two side-by-side images</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="leadspace-split-elements-group"/>
		</xsd:complexType>
	</xsd:element>	
	<!-- 5.3 11/29/05 jpp-egd:  Added <left-nav-include> element -->
	<!-- ================================================================ -->
	<!-- <left-nav-include> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="left-nav-include" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Specify a left navigation include</title>
				<desc>Specify an include with the appropriate left navigation for a page.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <li> element                                                     -->
	<!-- ================================================================ -->
	<xsd:element name="li">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an item in a list</title>
				<desc>Defines a list item for an ordered or unordered list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="dl"/>
				<!-- 6.0 Maverick R2 10/08/09 jpp: Added em tag -->
				<xsd:element ref="em"/>
				<xsd:element ref="figure"/>
				<xsd:element ref="font"/>
				<xsd:element ref="i"/>
				<xsd:element ref="img"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<xsd:element ref="pre"/>
				<!-- Maverick 6.0 R2 jpp-egd 062309: Added sidebar tag  -->
				<xsd:element ref="sidebar"/>
				<!-- 5.7 03/19/07 jpp (DR #2252):  Added <span> tag -->
				<xsd:element ref="span"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:element ref="table"/>
				<xsd:element ref="ul"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
				<!-- 4.0 4/26 tdc:  added font element per request #126 -->
			</xsd:choice>
			<xsd:attribute name="value" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>??? Is this the same as the HTML value attribute?  If so, this changes the item number for this item.  Subsequent list items are numbered accordingly as well. ???</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/03/08 jpp/egd Added link-block element -->
	<!-- ================================================================ -->
	<!-- <link-block> element                                                                                            -->
	<!-- ================================================================ -->
	<xsd:element name="link-block">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link block.</title>
				<desc>Element defines a link block, which consists of a link and associated content, such as an image.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="link-block-elements-group"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/03/08 jpp/egd Added link-block-abstract element -->
	<!-- ================================================================ -->
	<!-- <link-block-abstract> element                                                                                -->
	<!-- ================================================================ -->
	<xsd:element name="link-block-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Descriptive abstract for a module.</title>
				<desc>Descriptive abstract for a module that allows limited HTML tags for highlighting, simple formatting, special characters, and anchors.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="p"/>
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/03/08 jpp/egd Added link-list element -->
	<!-- ================================================================ -->
	<!-- <link-list> element                                                                                            -->
	<!-- ================================================================ -->
	<xsd:element name="link-list">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link list.</title>
				<desc>Element defines a link list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="link-list-elements-group"/>
			</xsd:choice>
			<xsd:attribute name="style" type="link-list-style" default="bullet">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the type of link style used for link list items.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added container-column element -->
	<!-- ================================================================ -->
	<!-- <link-section> element        		         									                    			-->
	<!-- ================================================================ -->
	<xsd:element name="link-section">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link section</title>
				<desc>Defines a section of a column that can contain a link block or link list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="link-block"/>
				<xsd:element ref="link-list"/>
			</xsd:choice>
			<xsd:attribute name="separator" type="yesno" default="no">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether a separator should be added above the element</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.7 llk 03/26 update link to english -->
	<!-- ================================================================ -->
	<!-- <link-to-english> element                                           							 -->
	<!-- ================================================================ -->
	<xsd:element name="link-to-english" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Link to English summary page.</title>
