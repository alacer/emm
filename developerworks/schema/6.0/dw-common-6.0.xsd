				<xsd:element ref="br"/>
				<!-- 6.0 Maverick R3 03/08/10 jpp: Added span tag  -->
				<xsd:element ref="span"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="colspan" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the number of columns this cell should span. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS class to be applied to this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- Maverick 6.0 R2 tdc 110409 (DR 3264): Added optional headers attribute  -->
			<xsd:attribute name="headers" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The headers attribute applied to this cell.  Used to associate a data cell with one or more headings.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- Maverick 6.0 R2 tdc 102609 (DR #3263): Added optional id attribute  -->
			<xsd:attribute name="id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The id attribute applied to this heading.  Used to associate a heading with one or more data cells.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="rowspan" type="xsd:positiveInteger">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the number of rows this cell should span.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="scope" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Attribute required for accessibility.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="style" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The CSS style to be applied to this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the width of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="valign">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical alignment of this cell.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.5.1 10/19/06 egd:  Added scope attribute for th -->
		</xsd:complexType>
	</xsd:element>
