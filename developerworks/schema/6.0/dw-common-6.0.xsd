    <!-- 6.0 xM r2.3 06/20/11 tdc:  Added text -->
	<!-- ================================================================ -->
	<!-- <text-with-special-chars>                                               									-->
	<!-- ================================================================ -->
	<xsd:complexType name="text-with-special-chars" mixed="true">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>text</title>
				<desc>Text with special characters</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice minOccurs="0" maxOccurs="unbounded">
			<xsd:group ref="specialCharacters"/>
