	<!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-step abstract element -->
	<!-- ================================================================ -->
	<!-- <kp-step-abstract element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="kp-step-abstract">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A abstract for a knowledge path step.</title>
				<desc>An abstract used to summarize a knowledge path step.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a" />
				<xsd:element ref="br" />
				<xsd:element ref="em" />
				<xsd:element ref="strong" />
				<xsd:element ref="sub" />
