<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:dw="http://www.ibm.com/developerWorks/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="1.0">
	<!-- ================================================================ -->
	<!-- <a> (anchor) element   -->
	<!-- ================================================================ -->
	<xsd:element name="a">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a hypertext anchor</title>
				<desc>Defines a hypertext link.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- 5.11 10/21/08 jpp: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="img"/>
				<!-- 6.0 Maverick R3 03/08/10 jpp: Added span tag  -->
				<xsd:element ref="span"/>
				<!-- 5.11 10/21/08 jpp: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the CSS class to be used for this anchor link. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="href" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the target URL of this hypertext link. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Gives this location a name.  Named anchors can be referenced just like external URLs.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="onClick" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the action that should take place when the user clicks this link.  This is typically JavaScript code or a call to a JavaScript function. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="onkeypress" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the action that should take place when the user clicks this link.  This is typically JavaScript code or a call to a JavaScript function. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="onmouseover" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the action that should take place when the user hovers over this link.  This is typically JavaScript code or a call to a JavaScript function. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="target" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines a target for this link.  You can use this attribute to refer to a named browser window.  A value of <tt>_blank</tt> means the linked document will appear in a new browser window.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.4 04/26 JP-EGD  Add the Class attribute to anchor tag DR #1877-->
			<!-- 5.6 12/05/2006 egd Added onkeypress attribute for a href so can code required links for SurfAid tagging -->
			<!-- 5.3 12/07/05 jpp-egd:  Added onmouseover for url tactic script on generic landing pages -->
			<!-- 4.0 6/1 tdc:  Removed the trackclick attribute per editors -->
		</xsd:complexType>
		<!-- Defined as ANY in the DTD -->
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <abstract> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="abstract" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>abstract</title>
				<desc>Brief overview of the document that will be displayed if no abstract-extended element is present, as well as in content views and search results; should contain information to help the user decide whether to read the document.  Used to create the meta abstract tag, which contributes to relevance in the ibm.com FAST search. Also used for e-mail to a friend.  No HTML coding allowed since this is used for the meta tags.  Use these guidelines to create: http://dw.raleigh.ibm.com/dwteam/search/search2003/guidelines/meta-guidelines.doc</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <abstract-extended> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="abstract-extended">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A long summary of the article</title>
				<desc>Optional, longer abstract that can be used as a page element of the document.  Use if you want a longer abstract or need to use HTML coding in the abstract that is a page element in the document.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="p"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag (RATLC00411737)  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
			<!-- 6.0 Maverick R3 03/01/10 jpp: Added frame attribute on abstract-extended element -->
			<xsd:attribute name="frame" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether a module should be framed or unframed; default is framed.  Only used within high definition landing pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 6.0 Maverick R3 03/01/10 jpp: Added position attribute on abstract-extended element -->
			<xsd:attribute name="position" type="alignment" default="center">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Identifies the position of the visible page abstract within high definition landing pages.  The default option - "center" - positions the abstract at the top of the center column.  The "left" option positions the abstract at the top of the left column in a side-by-side container module.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>			
		</xsd:complexType>
	</xsd:element>
	<!-- 5.9 08/23/07 tdc:  Added alttoc element -->
	<!-- ================================================================ -->
	<!-- <alttoc> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="alttoc">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>alttoc</title>
				<desc>Defines alternate text to appear in a table of contents (for example, a shorter version of a page title).  Has same definition as the title element, except for the refname attribute.  Defined as an element so it can contain special character elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<!-- inline -->
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <attribution> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="attribution">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the external source of an article</title>
				<desc>Credit line if source of the article is external, like an external magazine (JavaWorld), or Web site.</desc>
				<note>Use this value to create the DC.Publisher meta tag according to ibm.com meta tag standards.  If this value is not specified with a third party name, then fill in "IBM Corporation"</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
				<!-- <xsd:element ref="common-doc"/> -->
			</xsd:choice>
			<xsd:attribute name="type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The source of the article.  </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 12/1/03 TDC:  Removed default='javaworld'.  Should probably use an enumeration here in 2.2+ to trigger different attribution statements in xsl. -->
		</xsd:complexType>
	</xsd:element>
		<!-- Post xM (for V17) 11 09 11 egd: Added audio element -->
	<!-- ================================================================ -->
	<!-- <audio> element        		                      												-->
	<!-- ================================================================ -->
	<xsd:element name="audio">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Audio</title>
				<desc>Defines the audio and transcript URLs for this content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="audio-url" type="xsd:string" minOccurs="0"/>
			<xsd:element name="audio-transcript-url" type="xsd:string" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attributeGroup ref="itunes-feed-attributes"/>	
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed audience (summary) element as it is no longer used in 6.0 -->
	<!-- ================================================================ -->
	<!-- <author> element        														-->
	<!-- ================================================================ -->
	<xsd:element name="author">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define information about the author of the article</title>
				<desc>Can have multiple authors.  Future (post-3.0):  Authors can have multiple bios.  Currently "name" is used for author name in schema until investigation is complete on how to handle parts of name (prefix, firstname, middlename, lastname, suffix).</desc>
			</xsd:documentation>
		</xsd:annotation>
		<!-- 5.5.1 10/11/06 tdc:  DR 1957:  Added choice of cma-defined. -->
		<xsd:complexType>
			<xsd:choice>
				<xsd:element name="cma-defined" type="cma"/>
				<xsd:sequence>
					<!-- 5.12 03/11/09 egd:  Added author level for author recognition gifs -->
					<xsd:element name="author-level" type="author-level-type" minOccurs="0"/>
					<xsd:element ref="bio"/>
					<xsd:element ref="img" minOccurs="0"/>
					<xsd:choice>
						<xsd:element ref="name" minOccurs="0"/>
						<xsd:element name="author-name" type="dw-person-name" minOccurs="0"/>
					</xsd:choice>
					<xsd:element ref="company-name"/>
				</xsd:sequence>
			</xsd:choice>
			<xsd:attribute name="email" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author's email address.  Need some method so that author's e-mail address isn't visible on the page.  Once this requirement is met, should be Required.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="publish-email" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Lets editor choose whether or not to display the author's email address in the html.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="jobtitle" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The author's job title.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="email-cc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Allows someone on the dW team, like an acquisition editor, to be copied on author e-mails.  Need some method so that cc e-mail address isn't visible on the page.  Once this requirement is met, should be Required.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.5.1 9/27/06 keb:  Added publish-email attribute. -->
		</xsd:complexType>
	</xsd:element>
	<!-- USED FOR V6  -->
	<!-- ================================================================ 
	 <author-summary> element  
	================================================================ -->
	<xsd:element name="author-summary" type="contributor">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define information about the author of the summary</title>
				<desc>Authors can have multiple bios.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- xM 2.1 egd 04 15 11:  Removed author-tutorial-summary element as it's no longer used in 6.0 -->
	<!-- Maverick 6.0 R3 07 27 10 egd:  NOT USED FOR V6  Remove after summaries are migrated to V16. -->

	<!-- 12/15/05 fc:  added attendees -->
	<!-- ================================================================ -->
	<!-- <attendees> element                                             -->
	<!-- ================================================================ -->
	<xsd:element name="attendees">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the attending companies of an event</title>
				<desc>Defines the attending companies of an event.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="company-name"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <b> (bold) element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="b">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define boldfaced text</title>
				<desc>Defines boldfaced text. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="font"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<!-- 5.7 03/19/07 jpp (DR #2252):  Added <span> tag -->
				<xsd:element ref="span"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
				<!-- 4/7/03 Doug added <font> temporarily -->
			    	<!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
			<!-- Maverick 6.0 R2 jpp-egd 061509: Removing color class attribute from b tag -->
			<!-- <xsd:attribute name="class" type="colorname">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the color scheme used by this element's text.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute> -->
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <bio> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="bio">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<xsd:minLength value="10"/>
				<title>A short biography of the author</title>
				<desc>Defines a short biography of the author.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="p"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <blockquote> element                                             -->
	<!-- ================================================================ -->
	<xsd:element name="blockquote">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a lengthy quote</title>
				<desc>Defines a long quote.  The text is indented on the left and right margins.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<!-- 5.7 03/19/07 jpp (DR #2252):  Added <span> tag -->
				<xsd:element ref="span"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
				<!-- 5.2 8/31/05 fjc:  Added ul and ol for V. Suddaby -->
				<xsd:element ref="ul"/>
				<xsd:element ref="ol"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <br> (break) element                                             -->
	<!-- ================================================================ -->
	<xsd:element name="br">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a line break</title>
				<desc>Defines a line break in the text. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleType>
			<xsd:restriction base="empty"/>
		</xsd:simpleType>
	</xsd:element>
	<!-- 4.0 5/20 tdc:  removed breadcrumbappend element definition -->
	<!-- 5.2 8/22/05 tdc:  Removed <bundle-component> element -->
	<!-- 5.2 8/22/05 tdc:  Removed <bundle-components> element  -->
	<!-- 5.4 05/01/05 jp-egd:  Add brand image element to give editors option to show/not show the image on landing pages (DR #1793) -->
	<!-- ================================================================ -->
	<!-- <brand-image> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="brand-image">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Brand image</title>
				<desc>Brand image in right nav of landing pages.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:attribute name="display" type="yesno" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.0 6/21 tdc:  Added caption -->
	<!-- ================================================================ -->
	<!-- <caption> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="caption">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Caption</title>
				<desc>Used to describe table contents.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="em" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:group ref="specialCharacters"/>
			</xsd:choice>
			<xsd:attribute name="refname" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines an anchor for this heading.  You can use this attribute to reference this caption from elsewhere in the document.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.4 2/2/06 fjc add refname -->
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <code> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="code">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a code listing</title>
				<desc>The stylesheet allows code to be inline or section.  The contents of this element are displayed in a monospaced font, with all whitespace preserved from the original XML source.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="font"/>
				<xsd:element ref="heading"/>
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="type" type="inline" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The type of code listing.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The width in characters of this code listing.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <column-info> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="column-info">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define meta information for this column</title>
				<desc>Defines meta information for this column, such as an icon or a title.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="col-icon" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines an icon for this column.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="col-name" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines a name for this column.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- ** This is defined as ANY in the DTD, but it looks like it's empty. -->
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <company-name> element    -->
	<!-- ================================================================ -->
	<xsd:element name="company-name">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Company name</title>
				<desc>Defines the name of a company. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:group ref="specialCharacters"/>
				<!-- 4.0 12/09/04 fjc: we may want to change this to a URL element -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added container element -->
	<!-- ================================================================ -->
	<!-- <container> element                                  														-->
	<!-- ================================================================ -->
	<xsd:element name="container">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Module container</title>
				<desc>Defines a container within a module that has multiple containers.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- 6.0 Maverick R2 09/25/09 jpp: Changed the type reference from "link-name" to "module-heading" to add attributes required to link to the container heading -->
				<xsd:element name="container-heading" type="module-heading" minOccurs="0"/>
				<xsd:element name="container-forward-link" type="link" minOccurs="0"/>
				<!-- 6.0 Maverick R3 12-11-09 jpp: Added choice statement to allow selection of "container-html-body" element for high definition pages -->
				<xsd:choice>
					<xsd:element ref="container-body"/>
					<xsd:element ref="container-html-body"/>
				</xsd:choice>
			</xsd:sequence>
			<!-- 6.0 Maverick R3 12-11-09 jpp: Added frame attribute to enable high definition containers to be framed or unframed -->
			<xsd:attribute name="frame" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether a module should be framed or unframed; default is framed.  Only used within high definition pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- xM R2 (R2.3) jpp 07/22/11: Added module-orientation attribute (DR#3559) -->
			<xsd:attribute name="module-orientation" type="orientation" default="even-split">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether the module containers should be evenly split or offset (1/3 - 2/3); default is even-split.  Only used within high definition pages.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added container-body element -->
	<!-- ================================================================ -->
	<!-- <container-body> element        		         									            			-->
	<!-- ================================================================ -->
	<xsd:element name="container-body">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Container column.</title>
				<desc>Defines the contents of a module container.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice maxOccurs="unbounded">
				<xsd:element ref="link-section"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added container-column element -->
	<!-- ================================================================ -->
	<!-- <container-column> element        		         								        				-->
	<!-- ================================================================ -->
	<xsd:element name="container-column">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Container column</title>
				<desc>Defines a column within a single container module.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="link-section"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 10/02/09 jpp: Added container-html element -->
	<!-- ================================================================ -->
	<!-- <container-html> element    											                                    		   -->
	<!-- ================================================================ -->
	<xsd:element name="container-html">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Content module for a landing page.</title>
				<desc>Defines a content module within the main content area of a landing page that contains free-form HTML elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<!-- 6.0 Maverick R3 03/08/10 jpp: Added choice to allow container-html-body-hidef element -->
			<xsd:choice>
				<xsd:sequence>
					<xsd:element name="container-heading" type="module-heading"/>
					<!-- 6.0 Maverick R3 04 13 10 jpp/egd:  Changed name= to ref= -->
					<xsd:element ref="container-html-body"/>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:element ref="container-html-body-hidef"/>
				</xsd:sequence>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 10/02/09 jpp: Added container-html-body element -->
	<!-- ================================================================ -->
	<!-- <container-html-body> element                                                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="container-html-body">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>container-html-body element</title>
				<!-- 6.0 Maverick R3 12-11-09 jpp: Updated element description to reference "container" module -->
				<desc>Specifies the free-form HTML elements within a "container-html" or "container" module.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<!-- 6.0 Maverick R3 03/03/10 jpp: Added forward-link and forward-link-list elements -->
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>				
				<xsd:element ref="forward-link"/>
				<xsd:element ref="forward-link-list"/>
				<!-- 6.0 Maverick R3 05/18/10 jpp/egd: Updated choice list to allow a twisty section -->
				<xsd:element ref="twisty-section"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 03/08/09 jpp: Added container-html-body-hidef element -->
	<!-- ================================================================ -->
	<!-- <container-html-body-hidef> element                                                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="container-html-body-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>container-html-body-hidef element</title>
				<desc>Specifies the free-form HTML elements within a "container-html" module.  Used on high-definition landing pages.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
				<xsd:element ref="forward-link"/>
				<xsd:element ref="forward-link-list"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added container-link-block element -->
	<!-- ================================================================ -->
	<!-- <container-link-block> element        								                            			-->
	<!-- ================================================================ -->
	<xsd:element name="container-link-block">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Container link block.</title>
				<desc>Defines the elements for a container link block.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="container-link-block-elements-group"/>
			<xsd:attribute name="position" type="container-link-block-position" default="top">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specify the relative position of the tab for this page in the primary navigation.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.11 10/20/08 jpp:  Created unique group for container-link-block elements -->
			<!-- 5.11 10/01/08 jpp:  Modified default setting to 'top' -->
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added container-single element -->
	<!-- ================================================================ -->
	<!-- <container-single> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="container-single">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Single module container.</title>
				<desc>Defines the structure of a module with a single content container.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- 6.0 Maverick R2 09/25/09 jpp: Changed the type reference from "link-name" to "module-heading" to add attributes required to link to the container heading -->
				<xsd:element name="container-heading" type="module-heading" minOccurs="0"/>
				<xsd:element ref="container-link-block" minOccurs="0"/>
				<xsd:element name="container-forward-link" type="link" minOccurs="0"/>
				<xsd:element ref="container-column" minOccurs="0" maxOccurs="3"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 07/30/09 jpp/egd Added container-single-show-hide element -->
	<!-- ================================================================ -->
	<!-- <container-single-show-hide> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="container-single-show-hide">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Single module container with show/hide.</title>
				<desc>Defines the structure of a show/hide module that is a single content container.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<!-- 6.0 Mavaerick R2 09 15 09 egd:  Added date updated for this module -->
				<xsd:element ref="date-updated-module" minOccurs="0"/>
				<!-- 6.0 Maverick R2 09 13 09 egd:  Made container heading required -->
				<!-- 6.0 Maverick R2 09/25/09 jpp: Changed the type reference from "link-name" to "module-heading" to add attributes required to link to the container heading -->
				<xsd:element name="container-heading" type="module-heading"/>
				<xsd:element ref="link-list"/>
				<xsd:element name="more-featured-content" type="link" minOccurs="0"/>
				<xsd:element name="rss-link" type="link" minOccurs="0"/>				
			</xsd:sequence>
			<!-- 6.0 Maverick R2 09/13/09 egd Added show-date attribute -->
			<xsd:attribute name="show-date" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the show/hide module should include a date.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 07/30/09 jpp/egd Added container-single-show-hide-multipanel element -->
	<!-- ================================================================ -->
	<!-- <container-single-show-hide-multipanel> element        										                   				-->
	<!-- ================================================================ -->
	<xsd:element name="container-single-show-hide-multipanel">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Single module container with show/hide multipanel.</title>
				<desc>Defines the structure of a show/hide multipanel module that is a single content container.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="show-hide-panel" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	<!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary. -->
	<!-- ================================================================ -->
	<!-- <content-area-primary> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="content-area-primary">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the primary content area for this article</title>
				<desc>Indicates the primary dW product and/or technology content area that should contain this document.   Used to create page elements like breadcrumb trail. Search:  Request meta tag. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="name" type="content-area" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary. -->
	<!-- ================================================================ -->
	<!-- <content-area-secondary> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="content-area-secondary">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the secondary content area for this article</title>
				<desc>Indicates a secondary dW product and/or technology content area that should contain this document.  Request meta tag. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="name" type="content-area" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed contributor-summary summary element as it's no longer used in 6.0 -->
	<!-- 5.3 12/07/05 jpp-egd:  Added <content-area-subnav> element -->
	<!-- ================================================================ -->
	<!-- <content-area-subnav> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="content-area-subnav" type="link">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Subnavigation item in navigation trail</title>
				<desc>Include a subnavigation item in the navigation trail.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- 5.3 12/07/05 jpp-egd:  Added <content-area-subnav-sublevel> element -->
	<!-- ================================================================ -->
	<!-- <content-area-subnav-sublevel> element                                                  -->
	<!-- ================================================================ -->
	<xsd:element name="content-area-subnav-sublevel" type="link">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Sublevel page in navigation trail</title>
				<desc>Include a sublevel page in the navigation trail.  This page is under a subnavigation item in the navigation trail hierarchy, and would
        appear after the subnavigation item in the displayed navigation trail.</desc>
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <copyright> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="copyright">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A document copyright</title>
				<desc>Copyright of the spec.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
				<!-- <xsd:element ref="common-doc"/> -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed course-number (summary) element as it is no longer used in 6.0 -->
	<!-- 5.7 03/19/07 jpp (DR #2268):  Added customized-logo element -->
	<!-- ================================================================ -->
	<!-- <customized-logo> element                                       								-->
	<!-- ================================================================ -->
	<xsd:element name="customized-logo">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Customized logo</title>
				<desc>Defines a customized logo for the top right column of a landing page or pagegroup</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="img"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.0 6/21 tdc:  Added emphasis (em) -->
	<!-- 4.0 4/19 tdc: renamed date to date-published -->
	<!-- ================================================================ -->
	<!-- <date-published> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="date-published">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The date this article was published.</title>
				<desc>Date published on the supersite or local site.</desc>
				<note>Automatically update the DC.Date and IBM.Effective meta tags with this date - phase 2 of schema/stylesheet.  Date must be conform to ibm.com meta tag standards.</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attributeGroup ref="dmy"/>
		</xsd:complexType>
		<!-- ** Should be an XML Schema date type. -->
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <date-updated> element                                           -->
	<!-- ================================================================ -->
	<xsd:element name="date-updated">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Date the document's *content* is updated.</title>
				<desc>Date the document's *content* is updated.  Not a system date, and does not include updates for typos, etc.  Stylesheet automatically updates the d.c. date meta tag with this date, if present.  Date must be presented in international format.  Internally can have unbounded number of updates but UI should only show last update.</desc>
				<note>Automatically update the DC.Date and IBM.Effective meta tags with this date - phase 2 of schema/stylesheet.  Date must be conform to ibm.com meta tag standards.    Internally can have unbounded number of updates but UI should only show last update.</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attributeGroup ref="dmy"/>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 09 15 09 egd:  Added date-updated-module for simple show hide module -->
	<!-- ================================================================ -->
	<!-- <date-updated-module> element                                           -->
	<!-- ================================================================ -->
	<xsd:element name="date-updated-module">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Date the document's *module* is updated.</title>
				<desc>Date the document's *module* is updated.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attributeGroup ref="dmy"/>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 llk added date-translated element per Korea request DR 2869-->
	<!-- ================================================================ -->
	<!-- <date-translated> element                                           -->
	<!-- ================================================================ -->
	<xsd:element name="date-translated">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Date the document's *content* is translated and added to site.</title>
				<desc>Date the document's *content* is translated.  Not a system date, and does not include updates for typos, etc. Only used by local sites.</desc>
				<note/>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attributeGroup ref="dmy"/>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <dd> (definition) element                                        -->
	<!-- ================================================================ -->
	<xsd:element name="dd">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The definition of a term</title>
				<desc>This element contains the definition of a term in a definition list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="dl"/>
				<!-- Maverick 6.0 R2 jpp 10/09/09: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="p"/>
				<!-- Maverick 6.0 R2 jpp-egd 062309: Added sidebar tag  -->
				<xsd:element ref="sidebar"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<!-- Maverick 6.0 R2 jpp-egd 062309: Added table tag -->
				<xsd:element ref="table"/>
				<xsd:element ref="ul"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <dl> (definition list) element                                   -->
	<!-- ================================================================ -->
	<xsd:element name="dl">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A definition list</title>
				<desc>Defines a definition list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence maxOccurs="unbounded">
				<xsd:element ref="dt"/>
				<xsd:element ref="dd"/>
			</xsd:sequence>
			<xsd:attribute name="compact" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether or not this definition list should be compacted.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <docbody> element                                                -->
	<!-- ================================================================ -->
	<!-- ** Do we want this to contain text?  I don't think so... -->
	<xsd:element name="docbody">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the content of a document</title>
				<desc>Defines the content of a document.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<!-- Maverick 6.0 R2 jpp-egd 061509:  Removed mixed=true so text cannot be a child of docbody.  text can only be a child of sub-elements within docbody -->
		<xsd:complexType>
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:group ref="common-doc-group"/>
				<!-- <xsd:element ref="common-doc"/> -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 4.0 5/10 tdc:  use target-content instead of download-feature -->
	<!-- ================================================================ -->
	<!-- <dt> element                                                     -->
	<!-- ================================================================ -->
	<xsd:element name="dt">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a term</title>
				<desc>Defines a term in a definition list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed duration (summary) element as it is no longer used in 6.0 -->
	<!-- ================================================================ -->
	<!-- <em> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="em">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Emphasis</title>
				<desc>Used to apply emphasis to text.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">				
			<!-- Maverick 6.0 R2 jpp-egd 03 10 2010 (DR# 3350): Updating em to match name=i -->
				<xsd:element ref="a"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>				
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed enrollment-schedule (summary) element as it is no longer used in 6.0 -->
	<!-- 5.11 10/01/08 jpp/egd Added featured-content-module element -->
	<!-- ================================================================ -->
	<!-- <featured-content-module> element        										     				-->
	<!-- ================================================================ -->
	<xsd:element name="featured-content-module">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Featured content module for a landing page.</title>
				<desc>Defines a content module that holds featured content for a landing page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="leadspace"/>
				<!-- 6.0 Maverick R2 09/24/09 jpp: Added leadspace-decorative element reference -->
				<xsd:element ref="leadspace-decorative"/>
				<!-- xM R2 (R2.3) jpp 07/14/11: Added leadspace-split element reference -->
				<xsd:element ref="leadspace-split"/>
				<xsd:element ref="image-text-combination"/>
				<!-- 6.0 Maverick R2 09/24/09 jpp: Added image-text-combination-static element reference -->
				<xsd:element ref="image-text-combination-static"/>
				<xsd:element ref="leadspace-overlay"/>
				<xsd:element ref="rich-media"/>
				<!-- 5.12 llk 03/2009 - add link section to support russian top page feature area -->
				<xsd:element ref="image-list-combination"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R3 12-15-09 jpp: Added featured-content-module-hidef element for high definition pages -->
	<!-- ================================================================ -->
	<!-- <featured-content-module-hidef> element        										     				-->
	<!-- ================================================================ -->
	<xsd:element name="featured-content-module-hidef">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Featured content module for a high-definition landing page.</title>
				<desc>Defines a content module that holds featured content for a high-definition landing page.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<!-- 6.0 Maverick R3 02-15-09 jpp: Removed leadspace and leadspace-decoration selections for featured-content-module-hidef -->
				<!-- <xsd:element ref="leadspace"/> -->
				<!-- <xsd:element ref="leadspace-decorative"/> -->
				<xsd:element ref="leadspace-overlay"/>
				<xsd:element ref="rich-media"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <figure> element                                                 -->
	<!-- ================================================================ -->
	<xsd:element name="figure">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a illustrative detail</title>
				<desc>Define a code sample, a diagram, or some other illustrative detail.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="heading"/>
				<xsd:element ref="img" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
<!-- xM 2.1 egd 04 18 11:  Removed file-format and file-formats (summary) element as they are no longer used in 6.0 -->
	<!-- ================================================================ -->
	<!-- <font> element                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="font">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<desc>Defines the font to be used for this content. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<xsd:element ref="dl"/>
				<xsd:element ref="font"/>
				<xsd:element ref="i"/>
				<xsd:element ref="ol"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:element ref="table"/>
				<xsd:element ref="ul"/>
				<xsd:group ref="specialCharacters"/>
				<!-- <font> is evil, must be killed -->
			</xsd:choice>
			<xsd:attribute name="color" type="xslfoColor">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The color for the font.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="face" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The face name used for this font.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="size">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The size of the font. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed foreign-language (summary) element as it is no longer used in 6.0 -->
	<!-- 5.11 11/07/08 jpp/egd Added form element -->
	<!-- ================================================================ -->
	<!-- <form> element                                  														-->
	<!-- ================================================================ -->
	<xsd:element name="form">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Form</title>
				<desc>Defines a form within a container link block.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="form-heading" type="link-name" minOccurs="0"/>
				<xsd:element name="form-accessibility-heading" type="link-name"/>
				<xsd:element name="form-id" type="xsd:string" minOccurs="0"/>
				<xsd:element name="form-name" type="xsd:string" minOccurs="0"/>
				<xsd:element name="form-action" type="xsd:string"/>
				<xsd:element ref="form-input" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:choice>
					<xsd:element ref="pulldown-form"/>
					<xsd:element ref="search-form"/>
				</xsd:choice>
			</xsd:sequence>
			<xsd:attribute name="custom-form-name" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The name of a custom form that requires special processing within the stylesheet.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- <xsd:attribute name="type" type="form-type" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the type of form.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute> -->
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 11/07/08 jpp/egd Added form-input element -->
	<!-- ================================================================ -->
	<!-- <form-input> element        			                         											-->
	<!-- ================================================================ -->
	<xsd:element name="form-input">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Form input</title>
				<desc>Defines an input tag for a form.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:group ref="input-tag-group"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- 4.0 4/19 tdc: renamed forumurl to forum-url -->
	<!-- ================================================================ -->
	<!-- <forum-url> element                                               -->
	<!-- ================================================================ -->
	<xsd:element name="forum-url">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Associate a forum with this article</title>
				<desc>A document-specific forum link (discuss it icon)</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="url" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The URL of the associated forum.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="tactic" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the stylesheet should append the onmouseover for urltactic. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.7 03/13 egd Added attribute for urltactic URLs DR #2140 -->
			<!-- 5.7 0403 egd Removed default specification -->
		</xsd:complexType>
	</xsd:element>	
	<!-- 6.0 Maverick R3 03/03/10 jpp: Added forward-link element -->
	<!-- ================================================================ -->
	<!-- <forward-link>                                               -->
	<!-- ================================================================ -->
	<xsd:element name="forward-link">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>forward-link</title>
				<desc>Defines a forward link within a module that contains free-form HTML elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="text" type="link-name"/>
				<xsd:element name="url" type="xsd:string"/>
			</xsd:sequence>
			<xsd:attribute name="tactic" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether the stylesheet should append the onmouseover for urltactic.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 6.0 maverick r3 llk add new as attribute to forward-link element; only used by Japan -->
			<xsd:attribute name="new" type="yesno">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates that this content is new to the site; used by Japan only. </desc>
					</xsd:documentation>
				</xsd:annotation>
		</xsd:attribute>
			<xsd:attribute name="link-style" type="forward-link-style">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the style or icon to use with this forward link.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
	<!-- 6.0 Maverick R3 03/03/10 jpp: Added forward-link-list element -->
	<!-- ================================================================ -->
	<!-- <forward-link-list>                                               -->
	<!-- ================================================================ -->
	<xsd:element name="forward-link-list">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>forward-link-list</title>
				<desc>Defines a forward link list within a module that contains free-form HTML elements.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="forward-link" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="link-style" type="forward-link-style">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the style or icon to use on each forward link in the list.  The selection can be overridden by a selection on an individual forward link.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
	<!-- ================================================================ -->
	<!-- <heading> element                                                                                                   -->
	<!-- ================================================================ -->
	<xsd:element name="heading">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define a heading</title>
				<desc>Defines the text of a heading.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- 6.0 Maverick R3 01/29/09 jpp/egd: Added em tag  -->
				<xsd:element ref="em"/>
				<xsd:element ref="i"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
			<xsd:attribute name="alttoc" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines alternate text to be used in the table of contents.  This attribute is useful when the heading has lots of text. </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Names this heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="refname" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>????Defines an ID for this heading.  You can use this attribute to reference this heading from elsewhere in the document.???? </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="type" type="headingtype" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the type of heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="toc" type="yesno" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Deprecated in V5 - only here to ease CMA migration of pre-V5 content.  CMA team to remove this attribute from all XML files after OneX deadline.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="back-to-top" type="yesno" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates whether the stylesheet should add a Back to top link before the next major heading.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 5.0 5/26 tdc:  Added toc back for M. Chan to ease CMA migration of old content only.  CMA team to remove toc attribute from all XML files after OneX deadline met.  -->
			<!-- 5.2 8/25/05 tdc:  Added back-to-top -->
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <host> element                                             -->
	<!-- ================================================================ -->
	<xsd:element name="host">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define the host of an event</title>
				<desc>Defines the hosting company of an event.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="company-name"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <i> (italic) element                                             -->
	<!-- ================================================================ -->
	<xsd:element name="i">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define italicized text</title>
				<desc>Defines text that will appear in an italic font.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType mixed="true">
			<xsd:choice minOccurs="0" maxOccurs="unbounded">
				<xsd:element ref="a"/>
				<xsd:element ref="b"/>
				<xsd:element ref="br"/>
				<xsd:element ref="code"/>
				<!-- Maverick 6.0 R2 jpp-egd 061709: Added strong tag  -->
				<xsd:element ref="strong"/>
				<xsd:element ref="sub"/>
				<xsd:element ref="sup"/>
				<xsd:group ref="specialCharacters"/>
			            <!-- 2007-07-27 ibs Added automatic numbering and cross reference DR 3423 -->
			            <xsd:element ref="xref"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <id> element                                                     -->
	<!-- lk added content-id as string to support wsdd and db2 content ratings data -->
	<!-- ================================================================ -->
	<xsd:element name="id">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an ID for a document</title>
				<desc>A unique identification number for each piece of content that is generated by the content management team following a standard convention.</desc>
				<note>Future (post-4.0):  Make id an attrib of dw-document?  We'll only need one format across all document types, not multiple formats as currently defined.</note>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="domino-uid" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the unique Domino document ID for this document.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="cma-id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the unique content id used by the dW content management application.  This value is not used by ratings systems yet.  Once we're all using a common ratings system, this ID will be the only one we need and we can get rid of content-id and domino-uid.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="content-id" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the unique content id (non-Domino) for this document.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="original" type="yesno" default="yes">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Specifies whether the content associated with this ID is the original, or a translated version.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 4.0 7/1 tdc:  Added cma-id -->
			<!-- 4.0 4/29 tdc: Added to enable mapping of translated and original content -->
		</xsd:complexType>
	</xsd:element>
	<!-- 5.12 llk 03/2009 Added image-list-combination element -->
	<!-- ================================================================ -->
	<!-- <image-text-combination> element        		                      			    					-->
	<!-- ================================================================ -->
	<xsd:element name="image-list-combination">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Image and List Combination</title>
				<desc>Defines a format for a featured content module that includes an image and associated list.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="image-list-combination-elements-group"/>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 10/01/08 jpp/egd Added image-text-combination element -->
	<!-- ================================================================ -->
	<!-- <image-text-combination> element        		                      			    					-->
	<!-- ================================================================ -->
	<xsd:element name="image-text-combination">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Image and Text Combination</title>
				<desc>Defines a format for a featured content module that includes an image and associated text.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="image-text-combination-elements-group"/>
		</xsd:complexType>
	</xsd:element>
	<!-- 6.0 Maverick R2 09/24/09 jpp: Added image-text-combination-static element -->
	<!-- ================================================================ -->
	<!-- <image-text-combination-static> element        		                      			        		-->
	<!-- ================================================================ -->
	<xsd:element name="image-text-combination-static">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Image and Text Combination - Static</title>
				<desc>Defines a featured content module that is static -- does not include anchor links -- consisting of a decorative image and associated text.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:group ref="image-text-combination-static-elements-group"/>
		</xsd:complexType>
	</xsd:element>
	<!-- ================================================================ -->
	<!-- <img> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="img">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define an image</title>
				<desc>Defines an image that will appear in the article. </desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="align" type="alignment">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines how the image should be aligned.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="alt" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines alternate text for the graphic.  This text appears in non-graphical browsers and also may be used by accessibility aids such as screen readers.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="border" type="zeroOrOne">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines whether this image will have a border.  </desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- xM R2 (R2.3) jpp 07/25/11: Added class attribute (DR#3560) -->
			<xsd:attribute name="class" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines a class for the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="height" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the height in pixels of the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="hspace" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the horizontal space around the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines a name for the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="src" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the location of the image.  This can be a relative URL or an absolute URL.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="valign" type="vertical">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical alignment of the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="vspace" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the vertical space around the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="width">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the width in pixels of the image.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<!-- 4.0 05/24 tdc:  alt tag now required; conversion stylesheet will add one if not present already -->
		</xsd:complexType>
	</xsd:element>
	<!-- 5.3 11/30/05 JPP/EGD::  Added include element -->
	<!-- ================================================================ -->
	<!-- <include> element        -->
	<!-- ================================================================ -->
	<xsd:element name="include">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Include call</title>
				<desc>Add an include call to a file, form, etc.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="relative-url" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the relative URL of the target include file.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 5.11 11/07/08 jpp/egd Added input-field element -->
	<!-- ================================================================ -->
	<!-- <input-field> element        			                         											-->
	<!-- ================================================================ -->
	<xsd:element name="input-field">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Form input field</title>
				<desc>Defines the input field for a form.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:group ref="input-tag-group"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- xM 2.1 egd 04 18 11:  Removed instructor summary element as it's no longer used in 6.0 -->
	<!-- xM 2.1 egd 04 15 11:  Removed in-this-doc (summary) element as it's no longer used in 6.0 -->
	<!-- 5.5 9/7/06 keb:  Added <journal-info> element -->
	<!-- ================================================================ -->
	<!-- <journal-info> element                                            -->
	<!-- ================================================================ -->
	<xsd:element name="journal-info">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define information for this tech journal</title>
				<desc>Defines information for this journal, such as an icon or a title.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="journal-name" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the title for this journal.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="journal-icon" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines the icon for this journal.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="journal-url" type="xsd:string" use="optional">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Defines a url for this journal.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<!-- 4.0 4/22 tdc: added keywords for meta keywords (which are required, and other meta tags are not). -->
	<!-- ================================================================ -->
	<!-- <keywords> element (for meta keywords)                                       -->
	<!-- ================================================================ -->
	<xsd:element name="keywords">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>Define keywords for this article.</title>
				<desc>Used to create META keywords.  Used by FAST and other search engines.  Use these guidelines to create keywords:  http://dw.raleigh.ibm.com/dwteam/search/search2003/guidelines/meta-guidelines.html</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="content" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>The keywords, each separated by a comma.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
   <!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-next-step element -->
    <!-- ================================================================ -->
	<!-- <kp-next-step> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-next-step">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A knowledge path next step item</title>
				<desc>Knowledge path next step title and link for a specific on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="kp-next-step-title" type="text-with-special-chars"/>
				<xsd:element name="kp-next-step-url" type="xsd:string"/>
		   </xsd:sequence>
			<xsd:attribute name="kp-next-step-type" type="next-step-type" use="required"> 
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the action or type assoc. with the content, such as Read, Listen, Download, "Listen" for podcasts, etc.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="kp-next-step-pdf" type="yesno" use="optional" default="no">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates if the assoc. content is a PDF (which requires a special icon).  Default = no.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>		
		</xsd:complexType>
	</xsd:element>		
	
	<!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-next-steps element -->
    <!-- ================================================================ -->
	<!-- <kp-next-steps> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-next-steps">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of next steps for knowledge path documents</title>
				<desc>Collection of next steps, each containing links for further study on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="kp-next-step" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>	
	
	<!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-related-resource element -->
    <!-- ================================================================ -->
	<!-- <kp-related-resource> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-related-resource">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A related resource for knowledge path</title>
				<desc>A resource related to a knowledge path</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="kp-related-resource-title" type="text-with-special-chars"/>
				<xsd:element name="kp-related-resource-url" type="xsd:string"/>
		   </xsd:sequence>
			<xsd:attribute name="kp-related-resource-type" type="related-resource-type" use="required"> 
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the type assoc. with the content.  Needed to ensure correct icon.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>	
	
	<!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-related-resources element -->
    <!-- ================================================================ -->
	<!-- <kp-related-resources> element                                         												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-related-resources">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of knowledge path related resources</title>
				<desc>Collection of related resources (each containing resource links) that educate users on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="kp-related-resource" minOccurs="1" maxOccurs="6"/> <!-- 6 agreed to by Gretchen -->
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>	
	
	<!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-resource element -->
    <!-- ================================================================ -->
	<!-- <kp-resource> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-resource">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A knowledge path resource</title>
				<desc>Knowledge path resource title and link for a specific on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="kp-resource-title" type="text-with-special-chars"/>
				<!-- <xsd:element name="kp-resource-url" type="xsd:string"/> -->
				<xsd:element name="kp-resource-url" type="kp-resource-url-eld" />
				<xsd:element name="kp-resource-note" type="text-with-special-chars"/>
			</xsd:sequence>
			<xsd:attribute name="kp-resource-type" type="resource-type" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the action assoc. with the resource, such as "Read" for articles and tutorials, "Listen" for podcasts, etc.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="kp-resource-id" type="xsd:string" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the CMA ID of the resource.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="kp-resource-registration" type="resource-registration" use="required">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<desc>Indicates the CMA ID of the resource.</desc>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	
   <!-- 6.0 xM r2.3 08/08/11 tdc:  Added kp-resource-url-eld complexType -->
    <!-- ================================================================ -->
	<!-- <kp-resource-url-eld> complexType                                   												   -->
	<!-- ================================================================ -->	
       <xsd:complexType name="kp-resource-url-eld">
          <xsd:simpleContent>
	         <xsd:extension base="xsd:string">	            
		        <xsd:annotation>
			       <xsd:documentation xml:lang="en">
				      <title>kp-resource-url-eld</title>
				      <desc>Defines the url for the resource, also indicating if the url is for an external link doc (ELD) or not.</desc>
			       </xsd:documentation>
		        </xsd:annotation>
		        <xsd:attribute name="eld" type="yesno" use="required">
			          <xsd:annotation>
				         <xsd:documentation xml:lang="en">
					     <desc>Allows you specify whether the resource url points to an ELD or not.</desc>
				      </xsd:documentation>
			       </xsd:annotation>
			    </xsd:attribute>
			 </xsd:extension>		
		  </xsd:simpleContent>		
	   </xsd:complexType>
	
	
    <!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-resources element -->
    <!-- ================================================================ -->
	<!-- <kp-resources> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-resources">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A collection of knowledge path resources</title>
				<desc>Collection of resources (each containing resource links) that educate users on a topic</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:choice>
				<xsd:element ref="kp-resource" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:choice>
		</xsd:complexType>
	</xsd:element>

     <!-- 6.0 xM r2.3 06/20/11 tdc:  Added kp-step element -->
    <!-- ================================================================ -->
	<!-- <kp-step> element                                                 												   -->
	<!-- ================================================================ -->
	<xsd:element name="kp-step">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>A knowledge path step</title>
				<desc>A knowledge path step containing a link to a resource</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="heading" />
				<xsd:element ref="kp-step-abstract" />
				<xsd:element ref="kp-resources" />
