	<!-- 5.10 0229 egd: Added element for DC.Type meta tag DR #2788 -->
	<!-- ================================================================ -->
	<!-- <meta-dctype> element                                                    -->
	<!-- ================================================================ -->
	<xsd:element name="meta-dctype">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">
				<title>The DC.Type meta tag.</title>
				<desc>The meta tag that defines the content type of this content.</desc>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="cma-defined-type" type="cma"/>
			</xsd:sequence>
